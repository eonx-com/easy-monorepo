{
    "name": "eonx-com/easy-psr7-factory",
    "description": "Provides an easy way to create PSR7 Request/Response from Symfony Request/Response",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/psr-http-message-bridge": "^1.1 || ^2.0",
        "nyholm/psr7": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laravel/lumen-framework": "^5.8 || ^6.0 || ^8.0",
        "symfony/symfony": "^5.4 || ^6.0",
        "vlucas/phpdotenv": "^3.3 || ^5.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyPsr7Factory\\": "src",
            "EonX\\EasyPsr7Factory\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyPsr7Factory\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
