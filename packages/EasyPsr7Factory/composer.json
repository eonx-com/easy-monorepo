{
    "name": "eonx-com/easy-psr7-factory",
    "description": "Provides an easy way to create PSR7 Request/Response from Symfony Request/Response",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "nyholm/psr7": "^1.8",
        "symfony/psr-http-message-bridge": "^2.2"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^2.5",
        "laravel/lumen-framework": "^9.1",
        "phpunit/phpunit": "^10.2",
        "symfony/symfony": "^6.3",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyPsr7Factory\\": "src",
            "EonX\\EasyPsr7Factory\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyPsr7Factory\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-5.x": "5.0-dev"
        }
    }
}
