{
    "name": "eonx-com/easy-utils",
    "description": "EonX packages utils",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "eonx-com/easy-test": "^5.6.9",
        "laravel/lumen-framework": "^9.1",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.2",
        "symfony/symfony": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyUtils\\": "src",
            "EonX\\EasyUtils\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyUtils\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.7-dev"
        }
    }
}
