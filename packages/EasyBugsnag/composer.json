{
    "name": "eonx-com/easy-bugsnag",
    "description": "Ease integrating bugsnag in your PHP projects",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "bugsnag/bugsnag": "^3.29",
        "eonx-com/easy-utils": "^4.4",
        "nesbot/carbon": "^2.67",
        "nette/utils": "^3.2",
        "symfony/filesystem": "^6.2",
        "symfony/var-dumper": "^6.2"
    },
    "require-dev": {
        "doctrine/dbal": "^2.13",
        "doctrine/orm": "^2.14",
        "phpunit/phpunit": "^9.6 || ^10.2",
        "laravel/lumen-framework": "^9.1",
        "symfony/symfony": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyBugsnag\\": "src",
            "EonX\\EasyBugsnag\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyBugsnag\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
