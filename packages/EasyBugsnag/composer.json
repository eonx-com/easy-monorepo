{
    "name": "eonx-com/easy-bugsnag",
    "description": "Ease integrating bugsnag in your PHP projects",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "bugsnag/bugsnag": "^3.19",
        "eonx-com/easy-utils": "^3.2",
        "nesbot/carbon": "^1.39 || ^2.22",
        "nette/utils": "^3.1",
        "symfony/filesystem": "^4.4 || ^5.1.5",
        "symfony/var-dumper": "^4.4 || ^5.1.5"
    },
    "require-dev": {
        "doctrine/orm": "^2.6",
        "phpunit/phpunit": "^8.4 || ^9.5",
        "laravel/lumen-framework": "^5.5",
        "symfony/symfony": "^4.4 || ^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyBugsnag\\": "src",
            "EonX\\EasyBugsnag\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyBugsnag\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    }
}
