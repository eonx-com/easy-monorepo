{
    "name": "eonx-com/easy-bugsnag",
    "description": "Ease integrating bugsnag in your PHP projects",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "bugsnag/bugsnag": "^3.19",
        "eonx-com/easy-utils": "^4.5.1",
        "nesbot/carbon": "^2.22",
        "nette/utils": "^3.1",
        "symfony/filesystem": "^5.4 || ^6.0",
        "symfony/var-dumper": "^5.4 || ^6.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.6",
        "doctrine/orm": "^2.6",
        "phpunit/phpunit": "^9.5 || ^10.0",
        "laravel/lumen-framework": "^5.8 || ^6.0 || ^8.0",
        "symfony/symfony": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyBugsnag\\": "src",
            "EonX\\EasyBugsnag\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyBugsnag\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.6-dev"
        }
    }
}
