{
    "name": "eonx-com/easy-bugsnag",
    "description": "Ease integrating bugsnag in your PHP projects",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "bugsnag/bugsnag": "^3.29",
        "eonx-com/easy-utils": "^5.13",
        "guzzlehttp/guzzle": "^7.0",
        "nesbot/carbon": "^3.7",
        "nette/utils": "^3.2",
        "symfony/cache": "^7.0",
        "symfony/cache-contracts": "^3.5",
        "symfony/config": "^7.0",
        "symfony/console": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/event-dispatcher": "^7.0",
        "symfony/filesystem": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/messenger": "^7.0",
        "symfony/var-dumper": "^7.0"
    },
    "require-dev": {
        "doctrine/dbal": "^3.0",
        "laravel/lumen-framework": "^11.0",
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyBugsnag\\": "src",
            "EonX\\EasyBugsnag\\Bundle\\": "bundle",
            "EonX\\EasyBugsnag\\Laravel\\": "laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyBugsnag\\Tests\\": "tests",
            "EonX\\EasyBugsnag\\Tests\\Unit\\": "tests/Unit/src",
            "EonX\\EasyBugsnag\\Tests\\Unit\\Bundle\\": "tests/Unit/bundle",
            "EonX\\EasyBugsnag\\Tests\\Unit\\Laravel\\": "tests/Unit/laravel"
        }
    },
    "suggest": {
        "doctrine/dbal": "To log SQL queries in Bugsnag",
        "illuminate/contracts": "To use this package with Laravel/Lumen",
        "illuminate/http": "To use this package with Laravel/Lumen",
        "illuminate/queue": "To use this package with Laravel/Lumen",
        "illuminate/routing": "To use this package with Laravel/Lumen",
        "illuminate/support": "To use this package with Laravel/Lumen",
        "laravel/lumen-framework": "To use this package with Lumen"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.13-dev"
        }
    }
}
