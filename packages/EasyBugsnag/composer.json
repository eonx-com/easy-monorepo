{
    "name": "eonx-com/easy-bugsnag",
    "description": "Ease integrating bugsnag in your PHP projects",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "bugsnag/bugsnag": "^3.19",
        "eonx-com/easy-utils": "^4.4",
        "nesbot/carbon": "^2.22",
        "nette/utils": "^3.1",
        "symfony/filesystem": "^6.2",
        "symfony/var-dumper": "^6.2"
    },
    "require-dev": {
        "doctrine/dbal": "^2.6",
        "doctrine/orm": "^2.6",
        "phpunit/phpunit": "^9.5",
        "laravel/lumen-framework": "^9.0",
        "symfony/symfony": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyBugsnag\\": "src",
            "EonX\\EasyBugsnag\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyBugsnag\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
