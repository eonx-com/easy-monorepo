{
    "name": "eonx-com/easy-bugsnag",
    "description": "Ease integrating bugsnag in your PHP projects",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "bugsnag/bugsnag": "^3.19",
        "eonx-com/easy-utils": "^3.0",
        "nette/utils": "^3.1"
    },
    "require-dev": {
        "doctrine/orm": "^2.6",
        "phpunit/phpunit": "^8.4 || ^9.5",
        "laravel/lumen-framework": "^5.5",
        "symfony/symfony": "^4.4 || ^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyBugsnag\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyBugsnag\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
