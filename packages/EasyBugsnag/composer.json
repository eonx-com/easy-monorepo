{
    "name": "eonx-com/easy-bugsnag",
    "description": "Ease integrating bugsnag in your PHP projects",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "bugsnag/bugsnag": "^3.29",
        "eonx-com/easy-utils": "dev-feature/MTCE-221-Monorepo-v6-refactoring-EasyBugsnag",
        "guzzlehttp/guzzle": "^7.0",
        "nette/utils": "^3.2",
        "symfony/config": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/filesystem": "^6.4",
        "symfony/http-foundation": "^6.4",
        "symfony/http-kernel": "^6.4"
    },
    "require-dev": {
        "doctrine/dbal": "^3.0",
        "laravel/lumen-framework": "^9.1",
        "phpunit/phpunit": "^10.2",
        "symfony/cache": "^6.4",
        "symfony/yaml": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyBugsnag\\": "src",
            "EonX\\EasyBugsnag\\Bundle\\": "bundle",
            "EonX\\EasyBugsnag\\Laravel\\": "laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyBugsnag\\Tests\\": "tests",
            "EonX\\EasyBugsnag\\Tests\\Unit\\": "tests/Unit/src",
            "EonX\\EasyBugsnag\\Tests\\Unit\\Bundle\\": "tests/Unit/bundle",
            "EonX\\EasyBugsnag\\Tests\\Unit\\Laravel\\": "tests/Unit/laravel"
        }
    },
    "suggest": {
        "doctrine/dbal": "To log SQL queries in Bugsnag",
        "illuminate/contracts": "To use this package with Laravel/Lumen",
        "illuminate/http": "To use this package with Laravel/Lumen",
        "illuminate/queue": "To use this package with Laravel/Lumen",
        "illuminate/routing": "To use this package with Laravel/Lumen",
        "illuminate/support": "To use this package with Laravel/Lumen",
        "laravel/lumen-framework": "To use this package with Lumen",
        "nesbot/carbon": "To use this package with Symfony",
        "symfony/cache": "To use this package with Symfony",
        "symfony/cache-contracts": "To use this package with Symfony",
        "symfony/console": "To use this package with Symfony",
        "symfony/event-dispatcher": "To use this package with Symfony",
        "symfony/messenger": "To use this package with Symfony",
        "symfony/var-dumper": "To use this package with Symfony"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.12-dev"
        },
        "original-versions": {
            "eonx-com/easy-utils": "^5.12"
        }
