{
    "name": "eonx-com/easy-swoole",
    "description": "Ease running app with Open Swoole",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-utils": "5.0-dev",
        "symfony/process": "^6.3",
        "symfony/runtime": "^6.3",
        "symfony/string": "^6.3"
    },
    "require-dev": {
        "monolog/monolog": "^2.9",
        "phpunit/phpunit": "^9.6 || ^10.2",
        "symfony/symfony": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySwoole\\": "src",
            "EonX\\EasySwoole\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        },
        "files": [
            "src/Resources/functions/dump.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySwoole\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-5.x": "5.0-dev"
        }
    }
}
