{
    "name": "eonx-com/easy-swoole",
    "description": "Ease running app with Open Swoole",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-utils": "^4.5.5",
        "symfony/process": "^5.4 || ^6.0",
        "symfony/runtime": "^5.4 || ^6.0",
        "symfony/string": "^5.4 || ^6.0"
    },
    "require-dev": {
        "easycorp/easyadmin-bundle": "^3.5",
        "monolog/monolog": "^1.22 || ^2.0",
        "phpunit/phpunit": "^9.5 || ^10.0",
        "symfony/symfony": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySwoole\\": "src",
            "EonX\\EasySwoole\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        },
        "files": [
            "src/Resources/functions/dump.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySwoole\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.6-dev"
        }
    }
}
