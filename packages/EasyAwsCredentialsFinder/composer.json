{
    "name": "eonx-com/easy-aws-credentials-finder",
    "description": "Tool to ease finding aws credentials in project/machine",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-utils": "^4.5.1",
        "symfony/process": "^5.4 || ^6.0",
        "symplify/package-builder": "^10.1",
        "symplify/autowire-array-parameter": "^10.1",
        "aws/aws-sdk-php": "^3.134",
        "nesbot/carbon": "^2.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.0",
        "symfony/symfony": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyAwsCredentialsFinder\\": "src",
            "EonX\\EasyAwsCredentialsFinder\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyAwsCredentialsFinder\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.6-dev"
        }
    }
}
