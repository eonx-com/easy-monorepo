{
    "name": "eonx-com/easy-aws-credentials-finder",
    "description": "Tool to ease finding aws credentials in project/machine",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "eonx-com/easy-utils": "^3.0",
        "symfony/process": "^4.4 || ^5.1.5",
        "symplify/package-builder": "^8.3.41",
        "symplify/autowire-array-parameter": "^8.3.41",
        "aws/aws-sdk-php": "^3.134",
        "nesbot/carbon": "^2.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4 || ^9.5",
        "symfony/symfony": "^4.4 || ^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyAwsCredentialsFinder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyAwsCredentialsFinder\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
