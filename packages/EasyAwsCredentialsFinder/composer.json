{
    "name": "eonx-com/easy-aws-credentials-finder",
    "description": "Tool to ease finding aws credentials in project/machine",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-utils": "^4.4",
        "symfony/process": "^6.2",
        "symplify/package-builder": "^11.1",
        "symplify/autowire-array-parameter": "^11.1",
        "aws/aws-sdk-php": "^3.269",
        "nesbot/carbon": "^2.67"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "symfony/symfony": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyAwsCredentialsFinder\\": "src",
            "EonX\\EasyAwsCredentialsFinder\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyAwsCredentialsFinder\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
