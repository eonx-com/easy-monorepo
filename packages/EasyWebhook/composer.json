{
    "name": "eonx-com/easy-webhook",
    "description": "Sending webhooks has never been so easy (persistence, retry, async)",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-event-dispatcher": "^4.4",
        "eonx-com/easy-lock": "^4.4",
        "eonx-com/easy-random": "^4.4",
        "eonx-com/easy-pagination": "^4.4",
        "eonx-com/easy-utils": "^4.4",
        "nesbot/carbon": "^2.22",
        "nette/utils": "^3.1",
        "symfony/http-client": "^6.2"
    },
    "require-dev": {
        "doctrine/dbal": "^2.6",
        "illuminate/database": "^9.0",
        "ramsey/uuid": "^3.9 || ^4.0",
        "phpunit/phpunit": "^9.5",
        "laravel/lumen-framework": "^9.0",
        "symfony/symfony": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyWebhook\\": "src",
            "EonX\\EasyWebhook\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyWebhook\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
