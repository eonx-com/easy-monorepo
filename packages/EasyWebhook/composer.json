{
    "name": "eonx-com/easy-webhook",
    "description": "Sending webhooks has never been so easy (persistence, retry, async)",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-event-dispatcher": "^5.1",
        "eonx-com/easy-lock": "^5.1",
        "eonx-com/easy-pagination": "^5.1",
        "eonx-com/easy-random": "^5.1",
        "eonx-com/easy-utils": "^5.1",
        "nesbot/carbon": "^2.67",
        "nette/utils": "^3.2",
        "symfony/http-client": "^6.3"
    },
    "require-dev": {
        "doctrine/dbal": "^3.0",
        "illuminate/database": "^9.52",
        "laravel/lumen-framework": "^9.1",
        "phpunit/phpunit": "^10.2",
        "symfony/symfony": "^6.3",
        "symfony/uid": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyWebhook\\": "src",
            "EonX\\EasyWebhook\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyWebhook\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.1-dev"
        }
    }
}
