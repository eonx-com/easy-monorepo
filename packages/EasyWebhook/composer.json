{
    "name": "eonx-com/easy-webhook",
    "description": "Sending webhooks has never been so easy (persistence, retry, async)",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "eonx-com/easy-event-dispatcher": "^3.4.17",
        "eonx-com/easy-lock": "^3.4.17",
        "eonx-com/easy-random": "^3.4.17",
        "eonx-com/easy-pagination": "^3.4.17",
        "eonx-com/easy-utils": "^3.4.17",
        "nesbot/carbon": "^1.39 || ^2.22",
        "nette/utils": "^3.1",
        "symfony/http-client": "^4.4 || ^5.1.5"
    },
    "require-dev": {
        "doctrine/dbal": "^2.6",
        "illuminate/database": "^5.5 || ^6.20.26 || ^8.1",
        "ramsey/uuid": "^3.9 || ^4.0",
        "phpunit/phpunit": "^8.4 || ^9.5",
        "laravel/lumen-framework": "^5.5 || ^8.1",
        "symfony/symfony": "^4.4 || ^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyWebhook\\": "src",
            "EonX\\EasyWebhook\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyWebhook\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.5-dev"
        }
    }
}
