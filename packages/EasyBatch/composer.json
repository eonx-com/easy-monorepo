{
    "name": "eonx-com/easy-batch",
    "description": "Batching async processes made easy",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.0",
        "eonx-com/easy-encryption": "^5.2.1",
        "eonx-com/easy-event-dispatcher": "^5.2.1",
        "eonx-com/easy-pagination": "^5.2.1",
        "eonx-com/easy-random": "^5.2.1",
        "eonx-com/easy-utils": "^5.2.1",
        "nesbot/carbon": "^2.67"
    },
    "require-dev": {
        "eonx-com/easy-lock": "^5.2.1",
        "phpunit/phpunit": "^10.2",
        "psr/container": "^2.0",
        "symfony/symfony": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyBatch\\": "src",
            "EonX\\EasyBatch\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyBatch\\Tests\\": "tests"
        }
    },
    "suggest": {
        "eonx-com/easy-lock": "To lock async messages",
        "symfony/config": "To use this package with Symfony",
        "symfony/dependency-injection": "To use this package with Symfony",
        "symfony/http-kernel": "To use this package with Symfony",
        "symfony/messenger": "To use this package with Symfony"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.3-dev"
        }
    }
}
