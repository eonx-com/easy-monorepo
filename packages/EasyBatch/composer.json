{
    "name": "eonx-com/easy-batch",
    "description": "Batching async processes made easy",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-event-dispatcher": "5.0-dev",
        "eonx-com/easy-lock": "5.0-dev",
        "eonx-com/easy-pagination": "5.0-dev",
        "eonx-com/easy-random": "5.0-dev",
        "eonx-com/easy-utils": "5.0-dev",
        "nesbot/carbon": "^2.67"
    },
    "require-dev": {
        "doctrine/orm": "^2.14",
        "eonx-com/easy-encryption": "5.0-dev",
        "phpunit/phpunit": "^10.2",
        "ramsey/uuid": "^4.7",
        "symfony/symfony": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyBatch\\": "src",
            "EonX\\EasyBatch\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyBatch\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-5.x": "5.0-dev"
        }
    }
}
