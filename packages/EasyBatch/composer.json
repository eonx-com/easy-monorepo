{
    "name": "eonx-com/easy-batch",
    "description": "Batching async processes made easy",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "eonx-com/easy-event-dispatcher": "^3.4.24",
        "eonx-com/easy-lock": "^3.4.24",
        "eonx-com/easy-pagination": "^3.4.24",
        "eonx-com/easy-random": "^3.4.24",
        "nesbot/carbon": "^1.39 || ^2.22"
    },
    "require-dev": {
        "ramsey/uuid": "^3.9 || ^4.0",
        "doctrine/orm": "^2.6",
        "phpunit/phpunit": "^8.4 || ^9.5",
        "symfony/symfony": "^4.4 || ^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyBatch\\": "src",
            "EonX\\EasyBatch\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyBatch\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.4-dev"
        }
    }
}
