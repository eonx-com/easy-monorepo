{
    "name": "eonx-com/easy-batch",
    "description": "Batching async processes made easy",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.0",
        "eonx-com/easy-encryption": "dev-feature/MTCE-221-Monorepo-v6-refactoring-EasyBugsnag",
        "eonx-com/easy-event-dispatcher": "dev-feature/MTCE-221-Monorepo-v6-refactoring-EasyBugsnag",
