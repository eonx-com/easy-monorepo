{
    "name": "eonx-com/easy-batch",
    "description": "Batching async processes made easy",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-event-dispatcher": "^4.1",
        "eonx-com/easy-lock": "^4.1",
        "eonx-com/easy-pagination": "^4.1",
        "eonx-com/easy-random": "^4.1",
        "eonx-com/easy-utils": "^4.1",
        "nesbot/carbon": "^2.22"
    },
    "require-dev": {
        "ramsey/uuid": "^3.9 || ^4.0",
        "doctrine/orm": "^2.6",
        "phpunit/phpunit": "^9.5",
        "symfony/symfony": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyBatch\\": "src",
            "EonX\\EasyBatch\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyBatch\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
