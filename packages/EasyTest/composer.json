{
    "name": "eonx-com/easy-test",
    "description": "Makes testing easier",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-core": "^4.0",
        "symfony/console": "^5.4 || ^6.0",
        "nesbot/carbon": "^1.39 || ^2.22",
        "nette/utils": "^3.1",
        "symfony/intl": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "symfony/validator": "^5.4 || ^6.0",
        "symplify/autowire-array-parameter": "^8.3.41"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyTest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyTest\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/easy-test"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
