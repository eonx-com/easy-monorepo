{
    "name": "eonx-com/easy-test",
    "description": "Makes testing easier",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-core": "^4.4",
        "eonx-com/easy-utils": "^4.4",
        "symfony/console": "^5.4 || ^6.0",
        "nesbot/carbon": "^2.22",
        "nette/utils": "^3.1",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/intl": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "symfony/validator": "^5.4 || ^6.0",
        "symfony/yaml": "^5.4 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyTest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyTest\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/easy-test"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
