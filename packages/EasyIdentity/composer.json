{
    "name": "eonx-com/easy-identity",
    "description": "Tools to handle authentication like a pro",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "mockery/mockery": "^1.2",
        "ext-json": "*",
        "laravel/lumen-framework": "^5.5",
        "auth0/auth0-php": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyIdentity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyIdentity\\Tests\\": "tests"
        }
    },
    "suggest": {
        "auth0/auth0-php": "To use the Auth0 implementation to handle your users identity"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    }
}
