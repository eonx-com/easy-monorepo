{
    "name": "eonx-com/easy-logging",
    "description": "Create and configure Monolog Loggers easily",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-utils": "^4.4",
        "monolog/monolog": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "laravel/lumen-framework": "^9.1",
        "symfony/symfony": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyLogging\\": "src",
            "EonX\\EasyLogging\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyLogging\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
