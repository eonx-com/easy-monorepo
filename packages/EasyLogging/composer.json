{
    "name": "eonx-com/easy-logging",
    "description": "Create and configure Monolog Loggers easily",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "eonx-com/easy-utils": "^3.3",
        "monolog/monolog": "~1.22 || ~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4 || ^9.5",
        "laravel/lumen-framework": "^5.5",
        "laravel-doctrine/orm": "^1.4",
        "bugsnag/bugsnag": "^3.19",
        "bugsnag/bugsnag-laravel": "^2.17",
        "symfony/symfony": "^4.4 || ^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyLogging\\": "src",
            "EonX\\EasyLogging\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyLogging\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.3-dev"
        }
    }
}
