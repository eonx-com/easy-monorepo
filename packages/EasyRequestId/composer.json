{
    "name": "eonx-com/easy-request-id",
    "description": "Uniquely identify each request across multiple projects",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "eonx-com/easy-random": "^5.12",
        "eonx-com/easy-utils": "^5.12"
    },
    "require-dev": {
        "laravel/lumen-framework": "^9.1",
        "phpunit/phpunit": "^10.2",
        "symfony/framework-bundle": "^6.4",
        "symfony/uid": "^6.4",
        "symfony/yaml": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyRequestId\\": "src",
            "EonX\\EasyRequestId\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyRequestId\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.12-dev"
        }
    }
}
