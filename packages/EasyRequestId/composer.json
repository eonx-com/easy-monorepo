{
    "name": "eonx-com/easy-request-id",
    "description": "Uniquely identify each request across multiple projects",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-random": "^4.1.41",
        "eonx-com/easy-utils": "^4.1.41"
    },
    "require-dev": {
        "ramsey/uuid": "^3.9 || ^4.0",
        "phpunit/phpunit": "^9.5",
        "laravel/lumen-framework": "^5.8 || ^6.0 || ^8.0",
        "symfony/symfony": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyRequestId\\": "src",
            "EonX\\EasyRequestId\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyRequestId\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.2-dev"
        }
    }
}
