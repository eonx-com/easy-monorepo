{
    "name": "eonx-com/easy-request-id",
    "description": "Uniquely identify each request across multiple projects",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "eonx-com/easy-random": "^6.10",
        "symfony/config": "^7.1",
        "symfony/dependency-injection": "^7.1",
        "symfony/http-foundation": "^7.1.7",
        "symfony/http-kernel": "^7.1",
        "symfony/messenger": "^7.1"
    },
    "require-dev": {
        "laravel/lumen-framework": "^11.0",
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyRequestId\\": "src",
            "EonX\\EasyRequestId\\Bundle\\": "bundle",
            "EonX\\EasyRequestId\\Laravel\\": "laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyRequestId\\Tests\\": "tests",
            "EonX\\EasyRequestId\\Tests\\Unit\\": "tests/Unit/src",
            "EonX\\EasyRequestId\\Tests\\Unit\\Bundle\\": "tests/Unit/bundle",
            "EonX\\EasyRequestId\\Tests\\Unit\\Laravel\\": "tests/Unit/laravel"
        }
    },
    "suggest": {
        "eonx-com/easy-error-handler": "For EasyErrorHandler integration",
        "eonx-com/easy-http-client": "For EasyHttpClient integration",
        "eonx-com/easy-logging": "For EasyLogging integration",
        "eonx-com/easy-webhook": "For EasyWebhook integration",
        "twig/twig": "For Twig integration"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.10-dev"
        }
    }
}
