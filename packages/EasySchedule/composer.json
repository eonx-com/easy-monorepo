{
    "name": "eonx-com/easy-schedule",
    "description": "Provides the Command Scheduling logic of Laravel in a Symfony Console application",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "dragonmantank/cron-expression": "^3.3",
        "eonx-com/easy-event-dispatcher": "^5.12",
        "eonx-com/easy-lock": "^5.12",
        "nesbot/carbon": "^2.67",
        "symfony/config": "^6.4",
        "symfony/console": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/http-foundation": "^6.4",
        "symfony/http-kernel": "^6.4"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySchedule\\": "src",
            "EonX\\EasySchedule\\Bundle\\": "bundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySchedule\\Tests\\": "tests",
            "EonX\\EasySchedule\\Tests\\Unit\\": "tests/Unit/src"
        }
    },
    "suggest": {
        "doctrine/orm": "To clear the Doctrine Entity Manager"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.12-dev"
        }
    }
}
