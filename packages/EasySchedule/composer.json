{
    "name": "eonx-com/easy-schedule",
    "description": "Provides the Command Scheduling logic of Laravel in a Symfony Console application",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "dragonmantank/cron-expression": "^3.3",
        "eonx-com/easy-event-dispatcher": "^6.0.2",
        "eonx-com/easy-lock": "^6.0.2",
        "eonx-com/easy-utils": "^6.0.2",
        "nesbot/carbon": "^3.7",
        "symfony/config": "^7.0",
        "symfony/console": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/http-kernel": "^7.0"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySchedule\\": "src",
            "EonX\\EasySchedule\\Bundle\\": "bundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySchedule\\Tests\\": "tests",
            "EonX\\EasySchedule\\Tests\\Unit\\": "tests/Unit/src"
        }
    },
    "suggest": {
        "doctrine/persistence": "To clear the Doctrine Object Managers"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.1-dev"
        }
    }
}
