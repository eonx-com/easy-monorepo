{
    "name": "eonx-com/easy-schedule",
    "description": "Provides the Command Scheduling logic of Laravel in a Symfony Console application",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "nesbot/carbon": "^2.22",
        "dragonmantank/cron-expression": "^2.3",
        "symfony/console": "^4.2 || ^5.0",
        "eonx-com/easy-core": "^2.4.4"
    },
    "require-dev": {
        "symfony/symfony": "^4.2 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySchedule\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySchedule\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    }
}
