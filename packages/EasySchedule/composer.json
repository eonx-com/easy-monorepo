{
    "name": "eonx-com/easy-schedule",
    "description": "Provides the Command Scheduling logic of Laravel in a Symfony Console application",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "dragonmantank/cron-expression": "^3.3",
        "eonx-com/easy-event-dispatcher": "dev-feature/MTCE-221-Monorepo-v6-refactoring-EasyBugsnag",
        "eonx-com/easy-lock": "dev-feature/MTCE-221-Monorepo-v6-refactoring-EasyBugsnag",
        "nesbot/carbon": "^2.67",
        "symfony/console": "^6.4"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^10.2",
        "symfony/symfony": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySchedule\\": "src",
            "EonX\\EasySchedule\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySchedule\\Tests\\": "tests"
        }
    },
    "suggest": {
        "doctrine/orm": "To clear the Doctrine Entity Manager",
        "doctrine/persistence": "To clear the Doctrine Entity Manager",
        "symfony/config": "To use this package with Symfony",
        "symfony/dependency-injection": "To use this package with Symfony",
        "symfony/http-foundation": "To use this package with Symfony",
        "symfony/http-kernel": "To use this package with Symfony"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.12-dev"
        },
        "original-versions": {
            "eonx-com/easy-event-dispatcher": "^5.12",
            "eonx-com/easy-lock": "^5.12"
        }
    },
