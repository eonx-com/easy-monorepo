{
    "name": "eonx-com/easy-schedule",
    "description": "Provides the Command Scheduling logic of Laravel in a Symfony Console application",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "nesbot/carbon": "^1.39 || ^2.22",
        "dragonmantank/cron-expression": "^2.3",
        "symfony/console": "^4.4 || ^5.1.5",
        "eonx-com/easy-lock": "^3.3.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4 || ^9.5",
        "symfony/symfony": "^4.4 || ^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySchedule\\": "src",
            "EonX\\EasySchedule\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySchedule\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "3.4-dev"
        }
    }
}
