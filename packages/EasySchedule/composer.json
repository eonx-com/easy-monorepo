{
    "name": "eonx-com/easy-schedule",
    "description": "Provides the Command Scheduling logic of Laravel in a Symfony Console application",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "dragonmantank/cron-expression": "^3.3",
        "eonx-com/easy-event-dispatcher": "5.0-dev",
        "eonx-com/easy-lock": "5.0-dev",
        "nesbot/carbon": "^2.67",
        "symfony/console": "^6.3"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.6 || ^10.2",
        "symfony/symfony": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySchedule\\": "src",
            "EonX\\EasySchedule\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySchedule\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-5.x": "5.0-dev"
        }
    }
}
