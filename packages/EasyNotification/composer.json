{
    "name": "eonx-com/easy-notification",
    "description": "Client for dispatching notifications at EonX",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "^3.269",
        "eonx-com/easy-utils": "^4.4",
        "nette/utils": "^3.2",
        "symfony/http-client": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6 || ^10.2",
        "laravel/lumen-framework": "^9.1",
        "symfony/symfony": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyNotification\\": "src",
            "EonX\\EasyNotification\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyNotification\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
