{
    "name": "eonx-com/easy-notification",
    "description": "Client for dispatching notifications at EonX",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "^3.134",
        "eonx-com/easy-utils": "^4.3.16",
        "nette/utils": "^3.1",
        "symfony/http-client": "^5.4 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laravel/lumen-framework": "^5.8 || ^6.0 || ^8.0",
        "symfony/symfony": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyNotification\\": "src",
            "EonX\\EasyNotification\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyNotification\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
