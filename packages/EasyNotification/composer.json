{
    "name": "eonx-com/easy-notification",
    "description": "Client for dispatching notifications at EonX",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "aws/aws-sdk-php": "^3.269",
        "eonx-com/easy-utils": "^6.0",
        "nette/utils": "^4.0",
        "symfony/cache": "^7.0",
        "symfony/config": "^7.0",
        "symfony/contracts": "^3.5",
        "symfony/dependency-injection": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/http-kernel": "^7.0"
    },
    "require-dev": {
        "laravel/lumen-framework": "^11.0",
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyNotification\\": "src",
            "EonX\\EasyNotification\\Bundle\\": "bundle",
            "EonX\\EasyNotification\\Laravel\\": "laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyNotification\\Tests\\": "tests",
            "EonX\\EasyNotification\\Tests\\Unit\\": "tests/Unit/src",
            "EonX\\EasyNotification\\Tests\\Unit\\Bundle\\": "tests/Unit/bundle",
            "EonX\\EasyNotification\\Tests\\Unit\\Laravel\\": "tests/Unit/laravel"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0-dev"
        }
    }
}
