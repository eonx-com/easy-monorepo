{
    "name": "eonx-com/easy-notification",
    "description": "Client for dispatching notifications at EonX",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "aws/aws-sdk-php": "^3.134",
        "eonx-com/easy-utils": "^3.3",
        "nette/utils": "^3.1",
        "symfony/http-client": "^4.4 || ^5.1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4 || ^9.5",
        "laravel/lumen-framework": "^5.5",
        "symfony/symfony": "^4.4 || ^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyNotification\\": "src",
            "EonX\\EasyNotification\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyNotification\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.3-dev"
        }
    }
}
