{
    "name": "eonx-com/easy-event-dispatcher",
    "description": "Framework agnostic event dispatcher",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laravel/lumen-framework": "^9.1",
        "phpunit/phpunit": "^10.2",
        "symfony/symfony": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyEventDispatcher\\": "src",
            "EonX\\EasyEventDispatcher\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyEventDispatcher\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.2-dev"
        }
    }
}
