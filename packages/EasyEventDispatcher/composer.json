{
    "name": "eonx-com/easy-event-dispatcher",
    "description": "Framework agnostic event dispatcher",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "symfony/dependency-injection": "^7.2",
        "symfony/event-dispatcher-contracts": "^3.5",
        "symfony/http-kernel": "^7.2"
    },
    "require-dev": {
        "eonx-com/easy-test": "^6.18",
        "laravel/lumen-framework": "^11.0",
        "phpunit/phpunit": "^10.2|^11.4",
        "symfony/config": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyEventDispatcher\\": "src",
            "EonX\\EasyEventDispatcher\\Bundle\\": "bundle",
            "EonX\\EasyEventDispatcher\\Laravel\\": "laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyEventDispatcher\\Tests\\": "tests",
            "EonX\\EasyEventDispatcher\\Tests\\Unit\\Bundle\\": "tests/Unit/bundle",
            "EonX\\EasyEventDispatcher\\Tests\\Unit\\Laravel\\": "tests/Unit/laravel"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.18-dev"
        }
    }
}
