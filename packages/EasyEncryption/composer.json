{
    "name": "eonx-com/easy-encryption",
    "description": "Encryption made easy",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-sodium": "*",
        "eonx-com/easy-utils": "^5.12",
        "paragonie/halite": "^5.1",
        "symfony/config": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/http-kernel": "^6.4"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.269",
        "laravel/lumen-framework": "^9.1",
        "phpunit/phpunit": "^10.2",
        "symfony/filesystem": "^6.4",
        "symfony/process": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyEncryption\\": "src",
            "EonX\\EasyEncryption\\Bundle\\": "bundle",
            "EonX\\EasyEncryption\\Laravel\\": "laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyEncryption\\Tests\\": "tests",
            "EonX\\EasyEncryption\\Tests\\Unit\\": "tests/Unit/src",
            "EonX\\EasyEncryption\\Tests\\Unit\\Bundle\\": "tests/Unit/bundle",
            "EonX\\EasyEncryption\\Tests\\Unit\\Laravel\\": "tests/Unit/laravel"
        }
    },
    "suggest": {
        "aws/aws-sdk-php": "To use AWS Cloud HSM",
        "ext-pkcs11": "To use AWS Cloud HSM",
        "natepage/php-pkcs11-ide-helper": "To use IDE support when using PKCS11",
        "symfony/filesystem": "To use AWS Cloud HSM"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.12-dev"
        }
    }
}
