{
    "name": "eonx-com/easy-encryption",
    "description": "Encryption made easy",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "doctrine/orm": "^2.0",
        "eonx-com/easy-utils": "^5.12",
        "eonx-com/easy-utils": "^5.13",
        "paragonie/halite": "^5.1"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.269",
        "eonx-com/easy-test": "^5.13",
        "laravel/lumen-framework": "^9.1",
        "natepage/php-pkcs11-ide-helper": "^1.0",
        "phpunit/phpunit": "^10.2",
        "symfony/symfony": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyEncryption\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyEncryption\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.13-dev"
        }
    }
}
