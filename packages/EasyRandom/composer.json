{
    "name": "eonx-com/easy-random",
    "description": "Provides easy way to generate random values (string, int, uuids, ...)",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/string": "^6.4",
        "symfony/uid": "^6.4"
    },
    "require-dev": {
        "eonx-com/easy-test": "^5.11",
        "laravel/lumen-framework": "^9.1",
        "phpunit/phpunit": "^10.2",
        "symfony/framework-bundle": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyRandom\\": "src",
            "EonX\\EasyRandom\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyRandom\\Tests\\": "tests",
            "EonX\\EasyRandom\\Tests\\Bridge\\Symfony\\Fixtures\\App\\": "tests/Bridge/Symfony/Fixtures/app/src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.11-dev"
        }
    }
}
