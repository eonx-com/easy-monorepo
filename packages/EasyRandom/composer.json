{
    "name": "eonx-com/easy-random",
    "description": "Provides easy way to generate random values (string, int, uuids, ...)",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "symfony/string": "^4.4 || ^5.1.5"
    },
    "require-dev": {
        "ramsey/uuid": "^3.9",
        "phpunit/phpunit": "^8.4 || ^9.5",
        "laravel/lumen-framework": "^5.5",
        "symfony/symfony": "^4.4 || ^5.1.5",
        "symfony/uid": "^4.4 || ^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyRandom\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyRandom\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
