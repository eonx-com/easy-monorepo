{
    "name": "eonx-com/easy-random",
    "description": "Provides easy way to generate random values (string, int, uuids, ...)",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/string": "^6.3"
    },
    "require-dev": {
        "ramsey/uuid": "^4.7",
        "phpunit/phpunit": "^9.6 || ^10.2",
        "laravel/lumen-framework": "^9.1",
        "symfony/symfony": "^6.3",
        "symfony/uid": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyRandom\\": "src",
            "EonX\\EasyRandom\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyRandom\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
