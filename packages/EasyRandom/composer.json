{
    "name": "eonx-com/easy-random",
    "description": "Provides easy way to generate random values (string, int, uuids, ...)",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "symfony/string": "^4.4 || ^5.1.5 || 6.0.*"
    },
    "require-dev": {
        "ramsey/uuid": "^3.9 || ^4.0",
        "phpunit/phpunit": "^8.4 || ^9.5",
        "laravel/lumen-framework": "^5.5 || ^8.1",
        "symfony/symfony": "^4.4 || ^5.1.5 || 6.0.*",
        "symfony/uid": "^4.4 || ^5.1.5 || 6.0.*"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyRandom\\": "src",
            "EonX\\EasyRandom\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyRandom\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.5-dev"
        }
    }
}
