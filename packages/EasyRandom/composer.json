{
    "name": "eonx-com/easy-random",
    "description": "Provides easy way to generate random values (string, int, uuids, ...)",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "symfony/config": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/http-kernel": "^6.4",
        "symfony/string": "^6.4",
        "symfony/uid": "^6.4"
    },
    "require-dev": {
        "eonx-com/easy-test": "^5.12",
        "laravel/lumen-framework": "^10.0",
        "phpunit/phpunit": "^10.2",
        "symfony/framework-bundle": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyRandom\\": "src",
            "EonX\\EasyRandom\\Bundle\\": "bundle",
            "EonX\\EasyRandom\\Laravel\\": "laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyRandom\\Tests\\": "tests",
            "EonX\\EasyRandom\\Tests\\Fixture\\App\\": "tests/Fixture/app/src",
            "EonX\\EasyRandom\\Tests\\Unit\\": "tests/Unit/src",
            "EonX\\EasyRandom\\Tests\\Unit\\Bundle\\": "tests/Unit/bundle",
            "EonX\\EasyRandom\\Tests\\Unit\\Laravel\\": "tests/Unit/laravel"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.12-dev"
        }
    }
}
