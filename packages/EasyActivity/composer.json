{
    "name": "eonx-com/easy-activity",
    "description": "Tracking your record changes.",
    "type": "library",
    "license": "MIT",
    "require": {
        "ext-json": "*",
        "php": "^7.2 || ^8.0",
        "eonx-com/easy-random": "^3.4.26",
        "nesbot/carbon": "^1.39 || ^2.22"
    },
    "require-dev": {
        "doctrine/annotations": "^1.13",
        "doctrine/dbal": "^2.6",
        "doctrine/orm": "^2.6",
        "eonx-com/easy-doctrine": "^3.4.26",
        "phpunit/phpunit": "^8.4 || ^9.5",
        "ramsey/uuid": "^3.9 || ^4.0",
        "symfony/symfony": "^4.4 || ^5.1.5",
        "symfony/uid": "^4.4 || ^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyActivity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyActivity\\Tests\\": "tests"
        }
    },
    "suggest": {
        "symfony/serializer": "To serialize subject's data",
        "symfony/messenger": "To save log entities asynchronously",
        "eonx-com/easy-doctrine": "To create Log entries from to doctrine events"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.5-dev"
        }
    }
}
