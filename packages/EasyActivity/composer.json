{
    "name": "eonx-com/easy-activity",
    "description": "Tracking your record changes.",
    "type": "library",
    "license": "MIT",
    "require": {
        "ext-json": "*",
        "php": "^7.2 || ^8.0",
        "eonx-com/easy-doctrine": "^3.5",
        "eonx-com/easy-random": "^3.5",
        "nesbot/carbon": "^1.39 || ^2.22"
    },
    "require-dev": {
        "doctrine/dbal": "^2.6",
        "ramsey/uuid": "^3.9 || ^4.0",
        "symfony/symfony": "^4.4 || ^5.1.5",
        "symfony/uid": "^4.4 || ^5.1.5",
        "phpunit/phpunit": "^8.4 || ^9.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyActivity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyActivity\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.5-dev"
        }
    }
}
