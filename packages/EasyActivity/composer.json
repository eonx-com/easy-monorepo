{
    "name": "eonx-com/easy-activity",
    "description": "Tracking your record changes.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "doctrine/common": "^3.0",
        "nesbot/carbon": "^2.67"
    },
    "require-dev": {
        "doctrine/dbal": "^3.0",
        "doctrine/orm": "^2.14",
        "doctrine/persistence": "^3.0",
        "eonx-com/easy-doctrine": "^5.1",
        "eonx-com/easy-event-dispatcher": "^5.1",
        "eonx-com/easy-lock": "^5.1",
        "eonx-com/easy-webhook": "^5.1",
        "phpunit/phpunit": "^10.2",
        "psr/log": "^3.0",
        "symfony/config": "^6.3",
        "symfony/dependency-injection": "^6.3",
        "symfony/event-dispatcher": "^6.3",
        "symfony/filesystem": "^6.3",
        "symfony/http-kernel": "^6.3",
        "symfony/messenger": "^6.3",
        "symfony/property-access": "^6.3",
        "symfony/serializer": "^6.3",
        "symfony/uid": "^6.3",
        "symfony/yaml": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyActivity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyActivity\\Tests\\": "tests"
        }
    },
    "suggest": {
        "eonx-com/easy-doctrine": "To create Log entries from to doctrine events",
        "eonx-com/easy-lock": "To lock async messages",
        "symfony/messenger": "To save log entities asynchronously",
        "symfony/serializer": "To serialize subject's data"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.1-dev"
        }
    }
}
