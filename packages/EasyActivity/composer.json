{
    "name": "eonx-com/easy-activity",
    "description": "Tracking your record changes.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "doctrine/collections": "^1.7.2 || ^2.1",
        "doctrine/dbal": "^3.8",
        "doctrine/orm": "^2.20",
        "eonx-com/easy-doctrine": "^6.20",
        "eonx-com/easy-lock": "^6.20",
        "nesbot/carbon": "^3.8.4",
        "symfony/config": "^7.4",
        "symfony/dependency-injection": "^7.4",
        "symfony/event-dispatcher": "^7.4",
        "symfony/http-kernel": "^7.4",
        "symfony/messenger": "^7.4",
        "symfony/property-access": "^7.4",
        "symfony/serializer": "^7.4",
        "symfony/uid": "^7.4"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.12",
        "eonx-com/easy-encryption": "^6.20",
        "eonx-com/easy-test": "^6.20",
        "phpunit/phpunit": "^12.5",
        "symfony/framework-bundle": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyActivity\\": "src",
            "EonX\\EasyActivity\\Bundle\\": "bundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyActivity\\Tests\\": "tests",
            "EonX\\EasyActivity\\Tests\\Fixture\\App\\": "tests/Fixture/app/src",
            "EonX\\EasyActivity\\Tests\\Unit\\": "tests/Unit/src",
            "EonX\\EasyActivity\\Tests\\Unit\\Bundle\\": "tests/Unit/bundle"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.20-dev"
        }
    }
}
