{
    "name": "eonx-com/easy-activity",
    "description": "Tracking your record changes.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-random": "^4.1.21",
        "nesbot/carbon": "^2.22"
    },
    "require-dev": {
        "doctrine/annotations": "^1.13",
        "doctrine/dbal": "^2.6",
        "doctrine/orm": "^2.6",
        "eonx-com/easy-doctrine": "^4.1.21",
        "phpunit/phpunit": "^9.5",
        "ramsey/uuid": "^3.9 || ^4.0",
        "symfony/symfony": "^5.4 || ^6.0",
        "symfony/uid": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyActivity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyActivity\\Tests\\": "tests"
        }
    },
    "suggest": {
        "symfony/serializer": "To serialize subject's data",
        "symfony/messenger": "To save log entities asynchronously",
        "eonx-com/easy-doctrine": "To create Log entries from to doctrine events"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.2-dev"
        }
    }
}
