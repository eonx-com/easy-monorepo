{
    "name": "eonx-com/easy-activity",
    "description": "Tracking your record changes.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "doctrine/dbal": "^3.0",
        "doctrine/orm": "^2.14",
        "eonx-com/easy-doctrine": "^5.12",
        "eonx-com/easy-lock": "^5.12",
        "nesbot/carbon": "^3.7",
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/event-dispatcher": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/messenger": "^7.0",
        "symfony/property-access": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/uid": "^7.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.8",
        "eonx-com/easy-test": "^5.12",
        "phpunit/phpunit": "^10.2",
        "symfony/framework-bundle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyActivity\\": "src",
            "EonX\\EasyActivity\\Bundle\\": "bundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyActivity\\Tests\\": "tests",
            "EonX\\EasyActivity\\Tests\\Fixture\\App\\": "tests/Fixture/app/src",
            "EonX\\EasyActivity\\Tests\\Unit\\": "tests/Unit/src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.12-dev"
        }
    }
}
