{
    "name": "eonx-com/easy-activity",
    "description": "Tracking your record changes.",
    "type": "library",
    "license": "MIT",
    "require": {
        "doctrine/common": "^3.0",
        "nesbot/carbon": "^2.67",
        "php": "^8.1",
        "symfony/serializer": "^6.4"
    },
    "require-dev": {
        "doctrine/dbal": "^3.0",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/orm": "^2.14",
        "doctrine/persistence": "^3.0",
        "eonx-com/easy-doctrine": "dev-feature/MTCE-221-Monorepo-v6-refactoring-EasyBugsnag",
        "eonx-com/easy-event-dispatcher": "dev-feature/MTCE-221-Monorepo-v6-refactoring-EasyBugsnag",
        "eonx-com/easy-lock": "dev-feature/MTCE-221-Monorepo-v6-refactoring-EasyBugsnag",
        "eonx-com/easy-test": "dev-feature/MTCE-221-Monorepo-v6-refactoring-EasyBugsnag",
        "eonx-com/easy-webhook": "dev-feature/MTCE-221-Monorepo-v6-refactoring-EasyBugsnag",
        "phpunit/phpunit": "^10.2",
        "psr/log": "^3.0",
        "symfony/amazon-sqs-messenger": "^6.4",
        "symfony/doctrine-bridge": "^6.4",
        "symfony/framework-bundle": "^6.4",
        "symfony/monolog-bundle": "^3.8",
        "symfony/uid": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyActivity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyActivity\\Tests\\": "tests",
            "EonX\\EasyActivity\\Tests\\Bridge\\Symfony\\Fixtures\\App\\": "tests/Bridge/Symfony/Fixtures/app/src"
        }
    },
    "suggest": {
        "doctrine/dbal": "To save log entities to a database",
        "doctrine/orm": "To save log entities to a database",
        "eonx-com/easy-doctrine": "To create log entries from Doctrine events",
        "eonx-com/easy-lock": "To lock async messages",
