{
    "name": "eonx-com/easy-security",
    "description": "Provides security features to be generic across applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "nette/utils": "^3.2",
        "eonx-com/easy-api-token": "^4.4",
        "eonx-com/easy-utils": "^4.4"
    },
    "require-dev": {
        "eonx-com/easy-bugsnag": "^4.4",
        "eonx-com/easy-logging": "^4.4",
        "phpunit/phpunit": "^9.6 || ^10.2",
        "phpspec/prophecy": "^1.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "laravel/lumen-framework": "^9.1",
        "symfony/symfony": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySecurity\\": "src",
            "EonX\\EasySecurity\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySecurity\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
