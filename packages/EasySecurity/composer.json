{
    "name": "eonx-com/easy-security",
    "description": "Provides security features to be generic across applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "nette/utils": "^3.1",
        "eonx-com/easy-api-token": "^3.4",
        "eonx-com/easy-utils": "^3.4"
    },
    "require-dev": {
        "eonx-com/easy-bugsnag": "^3.4",
        "eonx-com/easy-logging": "^3.4",
        "phpunit/phpunit": "^8.4 || ^9.5",
        "laravel/lumen-framework": "^5.5 || ^8.1",
        "symfony/symfony": "^4.4 || ^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySecurity\\": "src",
            "EonX\\EasySecurity\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySecurity\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.4-dev"
        }
    }
}
