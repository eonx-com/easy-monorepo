        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^10.2",
        "symfony/symfony": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySecurity\\": "src",
            "EonX\\EasySecurity\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySecurity\\Tests\\": "tests"
        }
    },
    "extra": {
