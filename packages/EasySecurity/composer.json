{
    "name": "eonx-com/easy-security",
    "description": "Provides security features to be generic across applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "nette/utils": "^3.1",
        "eonx-com/easy-api-token": "^2.4.12",
        "eonx-com/easy-psr7-factory": "^2.4.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.4",
        "symfony/symfony": "^4.2 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySecurity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySecurity\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    }
}
