{
    "name": "eonx-com/easy-security",
    "description": "Provides security features to be generic across applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-api-token": "dev-feature/MTCE-221-Monorepo-v6-refactoring-EasyBugsnag",
        "eonx-com/easy-utils": "dev-feature/MTCE-221-Monorepo-v6-refactoring-EasyBugsnag",
        "nette/utils": "^3.2"
    },
    "require-dev": {
