{
    "name": "eonx-com/easy-security",
    "description": "Provides security features to be generic across applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "eonx-com/easy-api-token": "^5.12",
        "eonx-com/easy-utils": "^5.12",
        "nette/utils": "^3.2",
        "symfony/cache": "^6.4",
        "symfony/config": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/expression-language": "^6.4",
        "symfony/http-foundation": "^6.4",
        "symfony/http-kernel": "^6.4",
        "symfony/security-core": "^6.4",
        "symfony/security-http": "^6.4"
    },
    "require-dev": {
        "eonx-com/easy-bugsnag": "^5.12",
        "eonx-com/easy-logging": "^5.12",
        "laravel/lumen-framework": "^9.1",
        "phpspec/prophecy": "^1.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySecurity\\": "src",
            "EonX\\EasySecurity\\Bundle\\": "bundle",
            "EonX\\EasySecurity\\Laravel\\": "laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySecurity\\Tests\\": "tests",
            "EonX\\EasySecurity\\Tests\\Unit\\": "tests/Unit/src",
            "EonX\\EasySecurity\\Tests\\Unit\\Bundle\\": "tests/Unit/bundle",
            "EonX\\EasySecurity\\Tests\\Unit\\Laravel\\": "tests/Unit/laravel"
        }
    },
    "suggest": {
        "eonx-com/easy-bugsnag": "To use EasyBugsnag",
        "eonx-com/easy-logging": "To use EasyLogging"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.12-dev"
        }
    }
}
