{
    "name": "eonx-com/easy-security",
    "description": "Provides security features to be generic across applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "eonx-com/easy-api-token": "^6.20",
        "eonx-com/easy-utils": "^6.20",
        "nette/utils": "^4.0",
        "symfony/cache": "^7.4",
        "symfony/config": "^7.4",
        "symfony/dependency-injection": "^7.4",
        "symfony/expression-language": "^7.4",
        "symfony/framework-bundle": "^7.4",
        "symfony/http-foundation": "^7.4",
        "symfony/http-kernel": "^7.4",
        "symfony/routing": "^7.4",
        "symfony/security-core": "^7.4",
        "symfony/security-http": "^7.4"
    },
    "require-dev": {
        "eonx-com/easy-bugsnag": "^6.20",
        "eonx-com/easy-logging": "^6.20",
        "laravel/framework": "^11.44.1",
        "laravel/lumen-framework": "^11.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^12.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySecurity\\": "src",
            "EonX\\EasySecurity\\Bundle\\": "bundle",
            "EonX\\EasySecurity\\Laravel\\": "laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySecurity\\Tests\\": "tests",
            "EonX\\EasySecurity\\Tests\\Unit\\": "tests/Unit/src",
            "EonX\\EasySecurity\\Tests\\Unit\\Bundle\\": "tests/Unit/bundle",
            "EonX\\EasySecurity\\Tests\\Unit\\Laravel\\": "tests/Unit/laravel"
        }
    },
    "suggest": {
        "eonx-com/easy-bugsnag": "For EasyBugsnag integration",
        "eonx-com/easy-logging": "For EasyLogging integration"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.20-dev"
        }
    }
}
