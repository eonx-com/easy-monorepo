{
    "name": "eonx-com/easy-doctrine",
    "description": "Provides extensions to Doctrine 2",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "psr/event-dispatcher": "^1.0",
        "eonx-com/easy-error-handler": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4 || ^9.5",
        "phpspec/prophecy-phpunit": "^2.0",
        "doctrine/dbal": "^2.6",
        "doctrine/orm": "^2.6",
        "symfony/symfony": "^4.4 || ^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyDoctrine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyDoctrine\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.5-dev"
        }
    }
}
