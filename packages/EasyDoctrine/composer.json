{
    "name": "eonx-com/easy-doctrine",
    "description": "Provides functionalities to enhance Doctrine integration in your project",
    "keywords": [
        "doctrine"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-event-dispatcher": "^4.4",
        "eonx-com/easy-utils": "^4.4",
        "myclabs/deep-copy": "^1.11",
        "nesbot/carbon": "^2.67"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.269",
        "doctrine/annotations": "^1.14",
        "doctrine/dbal": "^2.13",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^2.14",
        "eonx-com/easy-error-handler": "^4.4",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.6 || ^10.2",
        "symfony/symfony": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyDoctrine\\": "src",
            "EonX\\EasyDoctrine\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyDoctrine\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
