{
    "name": "eonx-com/easy-doctrine",
    "description": "Provides functionalities to enhance Doctrine integration in your project",
    "keywords": [
        "doctrine"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "doctrine/collections": "^1.7.2 || ^2.1",
        "doctrine/dbal": "^3.8",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/orm": "^2.20",
        "eonx-com/easy-event-dispatcher": "^6.12.5",
        "eonx-com/easy-utils": "^6.12.5",
        "myclabs/deep-copy": "^1.11",
        "nesbot/carbon": "^3.8.4",
        "symfony/config": "^7.1",
        "symfony/contracts": "^3.5",
        "symfony/dependency-injection": "^7.1",
        "symfony/doctrine-bridge": "^7.1",
        "symfony/http-kernel": "^7.1"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.288.1",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "eonx-com/easy-bugsnag": "^6.12.5",
        "eonx-com/easy-error-handler": "^6.12.5",
        "eonx-com/easy-test": "^6.12.5",
        "mockery/mockery": "^1.6",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyDoctrine\\": "src",
            "EonX\\EasyDoctrine\\Bundle\\": "bundle",
            "EonX\\EasyDoctrine\\Laravel\\": "laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyDoctrine\\Tests\\": "tests",
            "EonX\\EasyDoctrine\\Tests\\Fixture\\App\\": "tests/Fixture/app/src",
            "EonX\\EasyDoctrine\\Tests\\Unit\\": "tests/Unit/src",
            "EonX\\EasyDoctrine\\Tests\\Unit\\Bundle\\": "tests/Unit/bundle"
        }
    },
    "suggest": {
        "aws/aws-sdk-php": "To use AWS RDS integration",
        "doctrine/doctrine-migrations-bundle": "To use integration with Doctrine Migrations",
        "eonx-com/easy-error-handler": "To report transactional errors",
        "illuminate/support": "To use this package with Laravel/Lumen"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.13-dev"
        }
    }
}
