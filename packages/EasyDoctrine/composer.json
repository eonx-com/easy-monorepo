{
    "name": "eonx-com/easy-doctrine",
    "description": "Provides functionalities to enhance Doctrine integration in your project",
    "keywords": ["doctrine"],
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-event-dispatcher": "^4.4.2",
        "eonx-com/easy-utils": "^4.4.2",
        "nesbot/carbon": "^2.22",
        "myclabs/deep-copy": "^1.11"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.134",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/dbal": "^2.6",
        "doctrine/orm": "^2.6",
        "eonx-com/easy-error-handler": "^4.4.2",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5 || ^10.0",
        "symfony/symfony": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyDoctrine\\": "src",
            "EonX\\EasyDoctrine\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyDoctrine\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.5-dev"
        }
    }
}
