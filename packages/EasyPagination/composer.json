{
    "name": "eonx-com/easy-pagination",
    "description": "Provides a generic way to handle pagination data from clients",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-utils": "^4.3.3",
        "laminas/laminas-uri": "^2.8"
    },
    "require-dev": {
        "doctrine/annotations": "^1.13",
        "doctrine/dbal": "^2.6",
        "doctrine/orm": "^2.6",
        "eonx-com/easy-psr7-factory": "^4.3.3",
        "phpunit/phpunit": "^9.5",
        "laravel/lumen-framework": "^5.8 || ^6.0 || ^8.0",
        "vlucas/phpdotenv": "^3.3 || ^5.2",
        "symfony/symfony": "^5.4 || ^6.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyPagination\\": "src",
            "EonX\\EasyPagination\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyPagination\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
