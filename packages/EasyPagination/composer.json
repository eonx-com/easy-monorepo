{
    "name": "eonx-com/easy-pagination",
    "description": "Provides a generic way to handle pagination data from clients",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "doctrine/dbal": "^3.0",
        "doctrine/orm": "^2.14",
        "eonx-com/easy-utils": "^6.1",
        "spatie/url": "^2.2",
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/string": "^7.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.12",
        "laravel/lumen-framework": "^11.0",
        "phpunit/phpunit": "^10.2",
        "symfony/uid": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyPagination\\": "src",
            "EonX\\EasyPagination\\Bundle\\": "bundle",
            "EonX\\EasyPagination\\Laravel\\": "laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyPagination\\Tests\\": "tests",
            "EonX\\EasyPagination\\Tests\\Unit\\": "tests/Unit/src",
            "EonX\\EasyPagination\\Tests\\Unit\\Bundle\\": "tests/Unit/bundle",
            "EonX\\EasyPagination\\Tests\\Unit\\Laravel\\": "tests/Unit/laravel"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
