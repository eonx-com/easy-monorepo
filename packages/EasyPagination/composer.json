{
    "name": "eonx-com/easy-pagination",
    "description": "Provides a generic way to handle pagination data from clients",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-utils": "^5.7",
        "spatie/url": "^2.2",
        "symfony/string": "^6.3"
    },
    "require-dev": {
        "doctrine/annotations": "^1.14",
        "doctrine/dbal": "^3.0",
        "doctrine/orm": "^2.14",
        "laravel/lumen-framework": "^9.1",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.2",
        "symfony/symfony": "^6.3",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyPagination\\": "src",
            "EonX\\EasyPagination\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyPagination\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.7-dev"
        }
    }
}
