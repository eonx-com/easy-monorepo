{
    "name": "eonx-com/easy-pagination",
    "description": "Provides a generic way to handle pagination data from clients",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "eonx-com/easy-psr7-factory": "^2.3.11",
        "phpunit/phpunit": "^7.5 || ^8.4",
        "laravel/lumen-framework": "^5.5",
        "vlucas/phpdotenv": "^3.3",
        "symfony/symfony": "^4.2 || ^5.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyPagination\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyPagination\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
