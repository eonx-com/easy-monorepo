{
    "name": "eonx-com/easy-pagination",
    "description": "Provides a generic way to handle pagination data from clients",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-utils": "^4.4",
        "laminas/laminas-uri": "^2.10"
    },
    "require-dev": {
        "doctrine/annotations": "^1.14",
        "doctrine/dbal": "^2.13",
        "doctrine/orm": "^2.14",
        "eonx-com/easy-psr7-factory": "^4.4",
        "phpunit/phpunit": "^9.6 || ^10.2",
        "laravel/lumen-framework": "^9.1",
        "vlucas/phpdotenv": "^5.5",
        "symfony/symfony": "^6.3",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyPagination\\": "src",
            "EonX\\EasyPagination\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyPagination\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
