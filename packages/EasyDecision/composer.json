{
    "name": "eonx-com/easy-decision",
    "description": "Your most complex decisions taken the easiest way",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-utils": "dev-feature/MTCE-221-Monorepo-v6-refactoring-EasyBugsnag",
        "psr/cache": "^3.0",
        "symfony/expression-language": "^6.4"
    },
    "require-dev": {
        "laravel/lumen-framework": "^9.1",
        "phpunit/phpunit": "^10.2",
        "symfony/symfony": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyDecision\\": "src",
            "EonX\\EasyDecision\\Bridge\\Symfony\\": "src/Bridge/Symfony",
            "EonX\\EasyDecision\\Expressions\\": "packages/expressions/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyDecision\\Expressions\\Tests\\": "packages/expressions/tests",
            "EonX\\EasyDecision\\Tests\\": "tests"
        }
    },
    "suggest": {
