{
    "name": "eonx-com/easy-decision",
    "description": "Your most complex decisions taken the easiest way",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "eonx-com/easy-utils": "^6.19",
        "psr/cache": "^3.0",
        "symfony/config": "^7.4",
        "symfony/dependency-injection": "^7.4",
        "symfony/expression-language": "^7.4",
        "symfony/http-foundation": "^7.4",
        "symfony/http-kernel": "^7.4",
        "symfony/messenger": "^7.4"
    },
    "require-dev": {
        "laravel/framework": "^11.44.1",
        "laravel/lumen-framework": "^11.0",
        "phpunit/phpunit": "^12.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyDecision\\": "src",
            "EonX\\EasyDecision\\Bundle\\": "bundle",
            "EonX\\EasyDecision\\Laravel\\": "laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyDecision\\Tests\\": "tests",
            "EonX\\EasyDecision\\Tests\\Unit\\": "tests/Unit/src",
            "EonX\\EasyDecision\\Tests\\Unit\\Bundle\\": "tests/Unit/bundle",
            "EonX\\EasyDecision\\Tests\\Unit\\Laravel\\": "tests/Unit/laravel"
        }
    },
    "suggest": {
        "illuminate/contracts": "To use this package with Laravel/Lumen",
        "illuminate/support": "To use this package with Laravel/Lumen"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.19-dev"
        }
    }
}
