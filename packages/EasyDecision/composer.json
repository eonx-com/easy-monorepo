{
    "name": "stepthefkup/easy-decision",
    "description": "Your most complex decisions taken the easiest way",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "stepthefkup/easy-pipeline": "^0.3.4",
        "symfony/expression-language": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "StepTheFkUp\\EasyDecision\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StepTheFkUp\\EasyDecision\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    }
}
