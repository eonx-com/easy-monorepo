{
    "name": "eonx-com/easy-decision",
    "description": "Your most complex decisions taken the easiest way",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "eonx-com/easy-utils": "^3.0.5",
        "symfony/expression-language": "^4.4 || ^5.1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4 || ^9.5",
        "laravel/lumen-framework": "^5.5",
        "symfony/symfony": "^4.4 || ^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyDecision\\": "src",
            "EonX\\EasyDecision\\Expressions\\": "packages/expressions/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyDecision\\Tests\\": "tests",
            "EonX\\EasyDecision\\Expressions\\Tests\\": "packages/expressions/tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
