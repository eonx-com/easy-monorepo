{
    "name": "eonx-com/easy-lock",
    "description": "Framework agnostic locking features",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/lock": "^6.3"
    },
    "conflict": {
        "dama/doctrine-test-bundle": "<7.1.1"
    },
    "require-dev": {
        "doctrine/orm": "^2.14",
        "phpunit/phpunit": "^9.6 || ^10.2",
        "laravel/lumen-framework": "^9.1",
        "symfony/symfony": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyLock\\": "src",
            "EonX\\EasyLock\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyLock\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
