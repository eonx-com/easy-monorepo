{
    "name": "eonx-com/easy-lock",
    "description": "Framework agnostic locking features",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/lock": "^5.4 || ^6.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.6",
        "phpunit/phpunit": "^9.5",
        "laravel/lumen-framework": "^5.8 || ^6.0 || ^8.0",
        "symfony/symfony": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyLock\\": "src",
            "EonX\\EasyLock\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyLock\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
