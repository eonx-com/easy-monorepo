{
    "name": "eonx-com/easy-serverless",
    "description": "Reusable functionalities to ease running PHP applications in serverless context",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "async-aws/secrets-manager": "^2.5"
    },
    "require-dev": {
        "async-aws/dynamo-db": "^3.6",
        "async-aws/sqs": "^2.7",
        "bref/bref": "^2.3.15",
        "bref/laravel-bridge": "^2.6",
        "bref/symfony-bridge": "^1.0.3",
        "bref/symfony-messenger": "^1.3.4",
        "laravel/lumen-framework": "^11.0",
        "monolog/monolog": "^3.7",
        "phpunit/phpunit": "^10.2|^11.4|^12.4",
        "symfony/cache": "^7.4",
        "symfony/config": "^7.4",
        "symfony/http-kernel": "^7.4"
    },
    "conflict": {
        "easycorp/easyadmin-bundle": "<4.13.1"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyServerless\\": "src",
            "EonX\\EasyServerless\\Bundle\\": "bundle",
            "EonX\\EasyServerless\\Laravel\\": "laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyServerless\\Tests\\": "tests",
            "EonX\\EasyServerless\\Tests\\Unit\\": "tests/Unit/src",
            "EonX\\EasyServerless\\Tests\\Unit\\Bundle\\": "tests/Unit/bundle",
            "EonX\\EasyServerless\\Tests\\Unit\\Laravel\\": "tests/Unit/laravel"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.19-dev"
        }
    }
}
