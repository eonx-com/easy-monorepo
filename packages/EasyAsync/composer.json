{
    "name": "eonx-com/easy-async",
    "description": "Makes dealing with async processes easier",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-event-dispatcher": "5.0-dev",
        "eonx-com/easy-pagination": "5.0-dev",
        "eonx-com/easy-random": "5.0-dev",
        "nesbot/carbon": "^2.67",
        "nette/utils": "^3.2"
    },
    "require-dev": {
        "doctrine/dbal": "^2.13 || 3.0",
        "doctrine/orm": "^2.14",
        "laravel/lumen-framework": "^9.1",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^9.6 || ^10.2",
        "ramsey/uuid": "^4.7",
        "symfony/symfony": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyAsync\\": "src",
            "EonX\\EasyAsync\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyAsync\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-5.x": "5.0-dev"
        }
    }
}
