{
    "name": "eonx-com/easy-async",
    "description": "Makes dealing with async processes easier",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "doctrine/collections": "^1.7.2 || ^2.1",
        "doctrine/dbal": "^4.3",
        "doctrine/orm": "^3.5",
        "doctrine/persistence": "^4.1",
        "psr/log": "^3.0",
        "symfony/config": "^7.2",
        "symfony/dependency-injection": "^7.2",
        "symfony/event-dispatcher": "^7.2",
        "symfony/http-kernel": "^7.2",
        "symfony/messenger": "^7.2"
    },
    "require-dev": {
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.12",
        "eonx-com/easy-event-dispatcher": "^6.16",
        "eonx-com/easy-logging": "^6.16",
        "eonx-com/easy-random": "^6.16",
        "eonx-com/easy-test": "^6.16",
        "phpunit/phpunit": "^10.2|^11.4",
        "symfony/doctrine-bridge": "^7.2",
        "symfony/framework-bundle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyAsync\\": "src",
            "EonX\\EasyAsync\\Bundle\\": "bundle",
            "EonX\\EasyAsync\\Laravel\\": "laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyAsync\\Tests\\": "tests",
            "EonX\\EasyAsync\\Tests\\Fixture\\App\\": "tests/Fixture/app/src",
            "EonX\\EasyAsync\\Tests\\Unit\\": "tests/Unit/src",
            "EonX\\EasyAsync\\Tests\\Unit\\Bundle\\": "tests/Unit/bundle"
        }
    },
    "suggest": {
        "eonx-com/easy-error-handler": "To report about the Illuminate worker stopping event",
        "eonx-com/easy-logging": "To provide the logging service",
        "illuminate/contracts": "To use this package with Laravel/Lumen",
        "illuminate/queue": "To use this package with Laravel/Lumen",
        "illuminate/support": "To use this package with Laravel/Lumen"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.16-dev"
        }
    }
}
