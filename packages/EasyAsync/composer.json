{
    "name": "eonx-com/easy-async",
    "description": "Makes dealing with async processes easier",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "eonx-com/easy-event-dispatcher": "^3.4.18",
        "eonx-com/easy-pagination": "^3.4.18",
        "eonx-com/easy-random": "^3.4.18",
        "nette/utils": "^3.1",
        "nesbot/carbon": "^1.39 || ^2.22"
    },
    "require-dev": {
        "ramsey/uuid": "^3.9 || ^4.0",
        "doctrine/dbal": "^2.6",
        "doctrine/orm": "^2.6",
        "phpunit/phpunit": "^8.4 || ^9.5",
        "symfony/symfony": "^4.4 || ^5.1.5",
        "mockery/mockery": "^1.4",
        "laravel/lumen-framework": "^5.5 || ^8.1"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyAsync\\": "src",
            "EonX\\EasyAsync\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyAsync\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.5-dev"
        }
    }
}
