{
    "name": "eonx-com/easy-async",
    "description": "Makes dealing with async processes easier",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-event-dispatcher": "^4.1.37",
        "eonx-com/easy-pagination": "^4.1.37",
        "eonx-com/easy-random": "^4.1.37",
        "nette/utils": "^3.1",
        "nesbot/carbon": "^2.22"
    },
    "require-dev": {
        "ramsey/uuid": "^3.9 || ^4.0",
        "doctrine/dbal": "^2.6",
        "doctrine/orm": "^2.6",
        "phpunit/phpunit": "^9.5",
        "symfony/symfony": "^5.4 || ^6.0",
        "mockery/mockery": "^1.4",
        "laravel/lumen-framework": "^5.8 || ^6.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyAsync\\": "src",
            "EonX\\EasyAsync\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyAsync\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.2-dev"
        }
    }
}
