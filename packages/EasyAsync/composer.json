{
    "name": "eonx-com/easy-async",
    "description": "Makes dealing with async processes easier",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-event-dispatcher": "^4.4",
        "eonx-com/easy-pagination": "^4.4",
        "eonx-com/easy-random": "^4.4",
        "nette/utils": "^3.2",
        "nesbot/carbon": "^2.67"
    },
    "require-dev": {
        "ramsey/uuid": "^4.7",
        "doctrine/dbal": "^2.13",
        "doctrine/orm": "^2.14",
        "phpunit/phpunit": "^9.6 || ^10.2",
        "symfony/symfony": "^6.2",
        "mockery/mockery": "^1.6",
        "laravel/lumen-framework": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyAsync\\": "src",
            "EonX\\EasyAsync\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyAsync\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
