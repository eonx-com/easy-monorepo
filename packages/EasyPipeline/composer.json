{
    "name": "eonx-com/easy-pipeline",
    "description": "Provides an easy and powerful way to implement pipelines for anything",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laravel/lumen-framework": "^9.1",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.2",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyPipeline\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyPipeline\\Tests\\": "tests"
        }
    },
    "suggest": {
        "illuminate/pipeline": "To use the Illuminate Pipeline implementation"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.12-dev"
        },
        "original-versions": []
    },
