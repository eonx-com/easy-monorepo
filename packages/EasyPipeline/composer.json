{
    "name": "eonx-com/easy-pipeline",
    "description": "Provides an easy and powerful way to implement pipelines for anything",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "laravel/lumen-framework": "^9.1",
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyPipeline\\": "src",
            "EonX\\EasyPipeline\\Laravel\\": "laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyPipeline\\Tests\\": "tests",
            "EonX\\EasyPipeline\\Tests\\Unit\\": "tests/Unit/src",
            "EonX\\EasyPipeline\\Tests\\Unit\\Laravel\\": "tests/Unit/laravel"
        }
    },
    "suggest": {
        "illuminate/pipeline": "To use the Illuminate Pipeline implementation"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.12-dev"
        }
    }
}
