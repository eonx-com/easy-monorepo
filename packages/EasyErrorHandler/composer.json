{
    "name": "eonx-com/easy-error-handler",
    "description": "Provides customizable ready-to-use error handler for applications",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-logging": "^5.12",
        "eonx-com/easy-utils": "^5.12",
        "nesbot/carbon": "^2.67",
        "nikic/php-parser": "^5.0"
    },
    "require-dev": {
        "api-platform/core": "^3.3",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/orm": "^2.14",
        "eonx-com/easy-bugsnag": "^5.12",
        "eonx-com/easy-test": "^5.12",
        "eonx-com/easy-webhook": "^5.12",
        "laravel/lumen-framework": "^9.1",
        "phpunit/phpunit": "^10.2",
        "symfony/browser-kit": "^6.4",
        "symfony/framework-bundle": "^6.4",
        "symfony/messenger": "^6.4",
        "symfony/routing": "^6.4",
        "symfony/serializer": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyErrorHandler\\": "src",
            "EonX\\EasyErrorHandler\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyErrorHandler\\Tests\\": "tests",
            "EonX\\EasyErrorHandler\\Tests\\Bridge\\Symfony\\Fixtures\\App\\": "tests/Bridge/Symfony/Fixtures/app/src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.12-dev"
        }
    }
}
