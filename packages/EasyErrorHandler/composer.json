{
    "name": "eonx-com/easy-error-handler",
    "description": "Provides customizable ready-to-use error handler for applications",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "eonx-com/easy-logging": "^3.4.20",
        "eonx-com/easy-utils": "^3.4.20",
        "nesbot/carbon": "^1.39 || ^2.22"
    },
    "require-dev": {
        "eonx-com/easy-bugsnag": "^3.4.20",
        "phpunit/phpunit": "^8.4 || ^9.5",
        "laravel/lumen-framework": "^5.5 || ^8.1",
        "symfony/symfony": "^4.4 || ^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyErrorHandler\\": "src",
            "EonX\\EasyErrorHandler\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyErrorHandler\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "3.5-dev"
        }
    }
}
