{
    "name": "eonx-com/easy-error-handler",
    "description": "Provides customizable ready-to-use error handler for applications",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-logging": "^5.1.5",
        "eonx-com/easy-utils": "^5.1.5",
        "nesbot/carbon": "^2.67",
        "nikic/php-parser": "^4.15"
    },
    "require-dev": {
        "eonx-com/easy-bugsnag": "^5.1.5",
        "laravel/lumen-framework": "^9.1",
        "phpunit/phpunit": "^10.2",
        "symfony/symfony": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyErrorHandler\\": "src",
            "EonX\\EasyErrorHandler\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyErrorHandler\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "5.2-dev"
        }
    }
}
