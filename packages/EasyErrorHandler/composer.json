{
    "name": "eonx-com/easy-error-handler",
    "description": "Provides customizable ready-to-use error handler for applications",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-logging": "^4.3.25",
        "eonx-com/easy-utils": "^4.3.25",
        "nesbot/carbon": "^2.22",
        "nikic/php-parser": "^4.15"
    },
    "require-dev": {
        "eonx-com/easy-bugsnag": "^4.3.25",
        "phpunit/phpunit": "^9.5 || ^10.0",
        "laravel/lumen-framework": "^5.8 || ^6.0 || ^8.0",
        "symfony/symfony": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyErrorHandler\\": "src",
            "EonX\\EasyErrorHandler\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyErrorHandler\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
