{
    "name": "eonx-com/easy-error-handler",
    "description": "Provides customizable ready-to-use error handler for applications",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "eonx-com/easy-logging": "^6.19",
        "eonx-com/easy-utils": "^6.19",
        "nesbot/carbon": "^3.8.4",
        "nikic/php-parser": "^5.0",
        "symfony/config": "^7.4",
        "symfony/console": "^7.4",
        "symfony/dependency-injection": "^7.4",
        "symfony/error-handler": "^7.4",
        "symfony/finder": "^7.4",
        "symfony/http-foundation": "^7.4",
        "symfony/http-kernel": "^7.4",
        "symfony/property-access": "^7.4",
        "symfony/serializer": "^7.4",
        "symfony/service-contracts": "^3.5",
        "symfony/translation": "^7.4"
    },
    "require-dev": {
        "eonx-com/easy-bugsnag": "^6.19",
        "eonx-com/easy-test": "^6.19",
        "eonx-com/easy-webhook": "^6.19",
        "laravel/framework": "^11.44.1",
        "laravel/lumen-framework": "^11.2",
        "phpunit/phpunit": "^12.5",
        "symfony/framework-bundle": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyErrorHandler\\": "src",
            "EonX\\EasyErrorHandler\\Bundle\\": "bundle",
            "EonX\\EasyErrorHandler\\Laravel\\": "laravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyErrorHandler\\Tests\\": "tests",
            "EonX\\EasyErrorHandler\\Tests\\Application\\": "tests/Application/src",
            "EonX\\EasyErrorHandler\\Tests\\Fixture\\App\\": "tests/Fixture/app/src",
            "EonX\\EasyErrorHandler\\Tests\\Unit\\": "tests/Unit/src",
            "EonX\\EasyErrorHandler\\Tests\\Unit\\Bundle\\": "tests/Unit/bundle",
            "EonX\\EasyErrorHandler\\Tests\\Unit\\Laravel\\": "tests/Unit/laravel"
        }
    },
    "suggest": {
        "api-platform/core": "To use ApiPlatform integration",
        "eonx-com/easy-bugsnag": "To use EasyBugsnag integration",
        "eonx-com/easy-webhook": "To use EasyWebhook integration",
        "symfony/messenger": "To use Symfony Messenger integration",
        "symfony/validator": "To use ApiPlatform integration"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.19-dev"
        }
    }
}
