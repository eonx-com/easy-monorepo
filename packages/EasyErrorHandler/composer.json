{
    "name": "eonx-com/easy-error-handler",
    "description": "Provides customizable ready-to-use error handler for applications",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-logging": "^4.0",
        "eonx-com/easy-utils": "^4.0",
        "nesbot/carbon": "^1.39 || ^2.22"
    },
    "require-dev": {
        "eonx-com/easy-bugsnag": "^4.0",
        "phpunit/phpunit": "^9.5",
        "laravel/lumen-framework": "^5.8 || ^6.0 || ^8.0",
        "symfony/symfony": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyErrorHandler\\": "src",
            "EonX\\EasyErrorHandler\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyErrorHandler\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
