{
    "name": "eonx-com/easy-core",
    "description": "Provides core features for Lumen applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "elasticsearch/elasticsearch": "^7.17",
        "eonx-com/easy-error-handler": "^5.1",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "doctrine/dbal": "^3.0",
        "doctrine/orm": "^2.14",
        "laravel/lumen-framework": "^9.1",
        "league/flysystem": "^1.1",
        "mockery/mockery": "^1.6",
        "phpspec/prophecy": "^1.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^10.2",
        "symfony/maker-bundle": "^1.48",
        "symfony/symfony": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyCore\\": "src",
            "EonX\\EasyCore\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyCore\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.1-dev"
        }
    }
}
