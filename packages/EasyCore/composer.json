{
    "name": "eonx-com/easy-core",
    "description": "Provides core features for Lumen applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "elasticsearch/elasticsearch": "^7.8",
        "eonx-com/easy-error-handler": "^4.3.7",
        "psr/event-dispatcher": "^1.0",
        "nesbot/carbon": "^2.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laravel/lumen-framework": "^5.8 || ^6.0 || ^8.0",
        "league/flysystem": "^1.1",
        "phpspec/prophecy": "^1.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "api-platform/core": "^2.6 || ^2.7",
        "doctrine/dbal": "^2.6",
        "doctrine/orm": "^2.6",
        "mockery/mockery": "^1.4",
        "symfony/symfony": "^5.4 || ^6.0",
        "symfony/maker-bundle": "^1.15"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyCore\\": "src",
            "EonX\\EasyCore\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyCore\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
