{
    "name": "eonx-com/easy-core",
    "description": "Provides core features for Lumen applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "elasticsearch/elasticsearch": "^7.8",
        "eonx-com/easy-error-handler": "^3.3",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4 || ^9.5",
        "laravel/lumen-framework": "^5.5",
        "league/flysystem": "^1.1",
        "phpspec/prophecy": "~1.0",
        "api-platform/core": "^2.5",
        "doctrine/orm": "^2.6",
        "mockery/mockery": "^1.2",
        "symfony/symfony": "^4.4 || ^5.1.5",
        "symfony/maker-bundle": "^1.15"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyCore\\": "src",
            "EonX\\EasyCore\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyCore\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.3-dev"
        }
    }
}
