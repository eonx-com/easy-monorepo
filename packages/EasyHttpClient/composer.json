{
    "name": "eonx-com/easy-http-client",
    "description": "Utils around HTTP client",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-event-dispatcher": "5.0-dev",
        "eonx-com/easy-utils": "5.0-dev",
        "nesbot/carbon": "^2.67"
    },
    "require-dev": {
        "bugsnag/bugsnag": "^3.29",
        "doctrine/dbal": "^2.13",
        "eonx-com/easy-lock": "5.0-dev",
        "eonx-com/easy-webhook": "5.0-dev",
        "laravel/lumen-framework": "^9.1",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^9.6 || ^10.2",
        "symfony/amazon-sqs-messenger": "^6.3",
        "symfony/contracts": "^3.3",
        "symfony/symfony": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyHttpClient\\": "src",
            "EonX\\EasyHttpClient\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyHttpClient\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-5.x": "5.0-dev"
        }
    }
}
