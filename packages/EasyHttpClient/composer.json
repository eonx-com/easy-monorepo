{
    "name": "eonx-com/easy-http-client",
    "description": "Utils around HTTP client",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "eonx-com/easy-event-dispatcher": "^3.3.12",
        "eonx-com/easy-utils": "^3.3.12",
        "nesbot/carbon": "^1.39 || ^2.22"
    },
    "require-dev": {
        "doctrine/dbal": "^2.6",
        "eonx-com/easy-lock": "^3.3.12",
        "eonx-com/easy-webhook": "^3.3.12",
        "laravel/lumen-framework": "^5.5",
        "phpunit/phpunit": "^8.4 || ^9.5",
        "symfony/symfony": "^4.4 || ^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyHttpClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyHttpClient\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.4-dev"
        }
    }
}
