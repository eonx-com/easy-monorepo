{
    "name": "eonx-com/easy-http-client",
    "description": "Utils around HTTP client",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-event-dispatcher": "^4.3.34",
        "eonx-com/easy-utils": "^4.3.34",
        "nesbot/carbon": "^2.22"
    },
    "require-dev": {
        "bugsnag/bugsnag": "^3.19",
        "doctrine/dbal": "^2.6",
        "eonx-com/easy-lock": "^4.3.34",
        "eonx-com/easy-webhook": "^4.3.34",
        "laravel/lumen-framework": "^5.8 || ^6.0 || ^8.0",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5 || ^10.0",
        "symfony/amazon-sqs-messenger": "^5.4 || ^6.0",
        "symfony/symfony": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyHttpClient\\": "src",
            "EonX\\EasyHttpClient\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyHttpClient\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
