{
    "name": "eonx-com/easy-http-client",
    "description": "Utils around HTTP client",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-event-dispatcher": "^5.11",
        "eonx-com/easy-utils": "^5.11",
        "nesbot/carbon": "^2.67"
    },
    "require-dev": {
        "bugsnag/bugsnag": "^3.29",
        "doctrine/dbal": "^4.0",
        "doctrine/orm": "^3.2",
        "doctrine/doctrine-bundle": "^2.12",
        "eonx-com/easy-lock": "^5.11",
        "eonx-com/easy-webhook": "^5.11",
        "eonx-com/easy-test": "^5.11",
        "laravel/lumen-framework": "^9.1",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.2",
        "symfony/amazon-sqs-messenger": "^6.4",
        "symfony/contracts": "^3.4",
        "symfony/symfony": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyHttpClient\\": "src",
            "EonX\\EasyHttpClient\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyHttpClient\\Tests\\": "tests",
            "EonX\\EasyHttpClient\\Tests\\Bridge\\Symfony\\Fixtures\\App\\": "tests/Bridge/Symfony/Fixtures/app/src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.11-dev"
        }
    }
}
