{
    "name": "eonx-com/easy-http-client",
    "description": "Utils around HTTP client",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "eonx-com/easy-event-dispatcher": "^4.1.5",
        "eonx-com/easy-utils": "^4.1.5",
        "nesbot/carbon": "^2.22"
    },
    "require-dev": {
        "doctrine/dbal": "^2.6",
        "eonx-com/easy-lock": "^4.1.5",
        "eonx-com/easy-webhook": "^4.1.5",
        "laravel/lumen-framework": "^5.8 || ^6.0 || ^8.0",
        "phpunit/phpunit": "^9.5",
        "symfony/symfony": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyHttpClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyHttpClient\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.2-dev"
        }
    }
}
