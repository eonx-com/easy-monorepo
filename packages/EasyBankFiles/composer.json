{
    "name": "eonx-com/easy-bank-files",
    "description": "Parsers/Generators for common bank files",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "eonx-com/easy-error-handler": "^6.20",
        "nette/utils": "^4.0"
    },
    "require-dev": {
        "eonx-com/easy-test": "^6.20",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^12.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyBankFiles\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyBankFiles\\Tests\\": "tests",
            "EonX\\EasyBankFiles\\Tests\\Unit\\": "tests/Unit/src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.20-dev"
        }
    }
}
