{
    "name": "eonx-com/easy-bank-files",
    "description": "Parsers/Generators for common bank files",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-dom": "*",
        "ext-libxml": "*",
        "eonx-com/easy-error-handler": "^5.6.3",
        "nette/utils": "^3.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyBankFiles\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyBankFiles\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.7-dev"
        }
    }
}
