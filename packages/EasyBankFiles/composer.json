{
    "name": "eonx-com/easy-bank-files",
    "description": "Parsers/Generators for common bank files",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "nette/utils": "^3.1",
        "eonx-com/easy-error-handler": "^4.0",
        "monolog/monolog": "^1.22 || ^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyBankFiles\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyBankFiles\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
