{
    "name": "eonx-com/easy-ssm",
    "description": "CLI tool to interact with AWS ParameterStore in a convenient way",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "^3.269",
        "eonx-com/easy-aws-credentials-finder": "^4.4",
        "eonx-com/easy-random": "^4.4",
        "nette/utils": "^3.2",
        "symfony/dotenv": "^6.2",
        "symfony/http-kernel": "^6.2",
        "symfony/yaml": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6 || ^10.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySsm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySsm\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/easy-ssm"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
