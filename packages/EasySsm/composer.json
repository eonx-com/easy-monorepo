{
    "name": "eonx-com/easy-ssm",
    "description": "CLI tool to interact with AWS ParameterStore in a convenient way",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "^3.134",
        "eonx-com/easy-aws-credentials-finder": "^4.1.23",
        "eonx-com/easy-random": "^4.1.23",
        "nette/utils": "^3.1",
        "symfony/dotenv": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "symfony/yaml": "^5.4 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySsm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySsm\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/easy-ssm"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "4.2-dev"
        }
    }
}
