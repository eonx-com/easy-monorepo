{
    "name": "eonx-com/easy-ssm",
    "description": "CLI tool to interact with AWS ParameterStore in a convenient way",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "^3.134",
        "eonx-com/easy-aws-credentials-finder": "^4.1",
        "eonx-com/easy-random": "^4.1",
        "nette/utils": "^3.1",
        "symplify/package-builder": "^10.1",
        "symplify/autowire-array-parameter": "^10.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/dotenv": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasySsm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasySsm\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/easy-ssm"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
