{
    "name": "eonx-com/easy-api-platform",
    "description": "Provides features for API Platform applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "api-platform/core": "^2.6 || ^2.7",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/orm": "^2.6",
        "doctrine/persistence": "^2.2 || ^3.0",
        "monolog/monolog": "^1.22 || ^2.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/http-foundation": "^5.4 || ^6.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.13",
        "mockery/mockery": "^1.4",
        "phpspec/prophecy": "^1.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5 || ^10.0",
        "symfony/filesystem": "^5.4 || ^6.0",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/symfony": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyApiPlatform\\": "src",
            "EonX\\EasyApiPlatform\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyApiPlatform\\Tests\\": "tests",
            "EonX\\EasyApiPlatform\\Tests\\Fixtures\\App\\": "tests/Fixtures/app/src"
        }
    },
    "conflict": {
        "api-platform/core": "<2.7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.6-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
