{
    "name": "eonx-com/easy-api-platform",
    "description": "Provides features for API Platform applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "api-platform/core": "^2.6 || ^2.7",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/http-foundation": "^5.4 || ^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5",
        "symfony/filesystem": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyApiPlatform\\": "src",
            "EonX\\EasyApiPlatform\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyApiPlatform\\Tests\\": "tests",
            "EonX\\EasyApiPlatform\\Tests\\Stubs\\App\\": "tests/Stubs/app/src"
        }
    },
    "conflict": {
        "api-platform/core": "<2.7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
