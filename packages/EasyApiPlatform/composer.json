{
    "name": "eonx-com/easy-api-platform",
    "description": "Provides features for API Platform applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "api-platform/core": "^3.4.17 || ^4.2.1",
        "doctrine/collections": "^1.7.2 || ^2.1",
        "doctrine/dbal": "^3.8",
        "doctrine/orm": "^2.20",
        "doctrine/persistence": "^3.3",
        "nesbot/carbon": "^3.8.4",
        "psr/log": "^3.0",
        "symfony/config": "^7.2",
        "symfony/dependency-injection": "^7.2",
        "symfony/http-foundation": "^7.2",
        "symfony/http-kernel": "^7.2",
        "symfony/property-access": "^7.2",
        "symfony/serializer": "^7.2",
        "symfony/uid": "^7.2"
    },
    "require-dev": {
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.12",
        "eonx-com/easy-bugsnag": "^6.15",
        "eonx-com/easy-error-handler": "^6.15",
        "eonx-com/easy-test": "^6.15",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^11.4",
        "symfony/browser-kit": "^7.2",
        "symfony/expression-language": "^7.2",
        "symfony/framework-bundle": "^7.2",
        "symfony/http-client": "^7.2",
        "symfony/security-bundle": "^7.2",
        "symfony/twig-bundle": "^7.2",
        "symfony/validator": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyApiPlatform\\": "src",
            "EonX\\EasyApiPlatform\\Bundle\\": "bundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyApiPlatform\\Tests\\": "tests",
            "EonX\\EasyApiPlatform\\Tests\\Application\\": "tests/Application/src",
            "EonX\\EasyApiPlatform\\Tests\\Fixture\\App\\": "tests/Fixture/app/src",
            "EonX\\EasyApiPlatform\\Tests\\Unit\\": "tests/Unit/src"
        }
    },
    "suggest": {
        "eonx-com/easy-bugsnag": "To use EasyBugsnag integration",
        "eonx-com/easy-error-handler": "To use EasyErrorHandler integration"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.15-dev"
        }
    }
}
