{
    "name": "eonx-com/easy-api-platform",
    "description": "Provides features for API Platform applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "api-platform/core": "^3.3.9",
        "doctrine/dbal": "^3.0",
        "doctrine/orm": "^2.14",
        "doctrine/persistence": "^3.0",
        "nesbot/carbon": "^2.67",
        "psr/log": "^3.0",
        "symfony/config": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/http-foundation": "^6.4",
        "symfony/http-kernel": "^6.4",
        "symfony/property-access": "^6.4",
        "symfony/serializer": "^6.4"
    },
    "require-dev": {
        "doctrine/annotations": "^1.14",
        "doctrine/doctrine-bundle": "^2.8",
        "eonx-com/easy-error-handler": "^5.12",
        "eonx-com/easy-test": "^5.12",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^10.2",
        "symfony/browser-kit": "^6.4",
        "symfony/framework-bundle": "^6.4",
        "symfony/http-client": "^6.4",
        "symfony/twig-bundle": "^6.4",
        "symfony/validator": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyApiPlatform\\": "src",
            "EonX\\EasyApiPlatform\\Bundle\\": "bundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyApiPlatform\\Tests\\": "tests",
            "EonX\\EasyApiPlatform\\Tests\\Application\\": "tests/Application/src",
            "EonX\\EasyApiPlatform\\Tests\\Fixture\\App\\": "tests/Fixture/app/src",
            "EonX\\EasyApiPlatform\\Tests\\Unit\\": "tests/Unit/src"
        }
    },
    "suggest": {
        "eonx-com/easy-error-handler": "To use EasyErrorHandler integration"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.12-dev"
        }
    }
}
