{
    "name": "eonx-com/easy-api-platform",
    "description": "Provides features for API Platform applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "api-platform/core": "^3.1",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^2.14",
        "doctrine/persistence": "^2.2",
        "monolog/monolog": "^2.9",
        "symfony/dependency-injection": "^6.3",
        "symfony/http-foundation": "^6.3"
    },
    "require-dev": {
        "doctrine/annotations": "^1.14",
        "mockery/mockery": "^1.6",
        "phpspec/prophecy": "^1.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.6 || ^10.2",
        "symfony/config": "^6.3",
        "symfony/filesystem": "^6.3",
        "symfony/symfony": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyApiPlatform\\": "src",
            "EonX\\EasyApiPlatform\\Bridge\\Symfony\\": "src/Bridge/Symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyApiPlatform\\Tests\\": "tests",
            "EonX\\EasyApiPlatform\\Tests\\Fixtures\\App\\": "tests/Fixtures/app/src"
        }
    },
    "conflict": {
        "api-platform/core": "<3.1"
    },
    "extra": {
        "branch-alias": {
            "dev-5.x": "5.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
