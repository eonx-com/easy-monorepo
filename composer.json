{
    "name": "eonx-com/easy-monorepo",
    "type": "library",
    "description": "Reusable packages to make PHP projects easier to create/maintain",
    "license": "MIT",
    "bin": [
        "packages/EasySsm/bin/easy-ssm",
        "packages/EasyTest/bin/easy-test"
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "ext-simplexml": "*",
        "doctrine/orm": "^2.6",
        "nesbot/carbon": "^1.39 || ^2.22",
        "dragonmantank/cron-expression": "^2.3 || ^3.0",
        "ramsey/uuid": "^3.9 || ^4.0",
        "symfony/cache": "^4.4 || ^5.1.5",
        "symfony/console": "^4.4 || ^5.1.5",
        "symfony/dotenv": "^4.4 || ^5.1.5",
        "symfony/expression-language": "^4.4 || ^5.1.5",
        "symfony/filesystem": "^4.4 || ^5.1.5",
        "symfony/finder": "^4.4 || ^5.1.5",
        "symfony/http-client": "^4.4 || ^5.1.5",
        "symfony/process": "^4.4 || ^5.1.5",
        "symfony/string": "^4.4 || ^5.1.5",
        "symfony/uid": "^4.4 || ^5.1.5",
        "symfony/psr-http-message-bridge": "^1.1 || ^2.0",
        "nyholm/psr7": "^1.1",
        "nette/utils": "^3.1",
        "aws/aws-sdk-php": "^3.134",
        "illuminate/support": "^5.5 || ^6.0 || ^8.0",
        "bugsnag/bugsnag": "^3.19",
        "elasticsearch/elasticsearch": "^7.8",
        "psr/event-dispatcher": "^1.0",
        "laminas/laminas-uri": "^2.8",
        "doctrine/migrations": "^3.2",
        "paragonie/halite": "^4.8"
    },
    "require-dev": {
        "api-platform/core": "^2.5",
        "auth0/auth0-php": "^5.4 || ^7.6",
        "bugsnag/bugsnag-laravel": "^2.17",
        "doctrine/dbal": "^2.6",
        "firebase/php-jwt": "^5.0",
        "laravel-doctrine/orm": "^1.4",
        "laravel/lumen-framework": "^5.5 || ^8.1",
        "mikey179/vfsstream": "~1",
        "mockery/mockery": "^1.4",
        "phpspec/prophecy": "~1.0",
        "symplify/phpstan-extensions": "^8.3.41",
        "phpunit/phpunit": "^8.4 || ^9.5",
        "psr/container": "^1.0",
        "symfony/symfony": "^4.4 || ^5.1.5",
        "symplify/monorepo-builder": "^8.3.41",
        "symplify/changelog-linker": "^8.3.41",
        "symfony/maker-bundle": "^1.15",
        "symfony/monolog-bundle": "^3.5",
        "squizlabs/php_codesniffer": "^3.6",
        "tracy/tracy": "^2.7",
        "migrify/vendor-patches": "^0.3.48",
        "dg/bypass-finals": "^1.2",
        "league/flysystem": "^1.1",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpseclib/phpseclib": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyMonorepo\\": "monorepo",
            "EonX\\EasyActivity\\": "packages/EasyActivity/src",
            "EonX\\EasyApiToken\\": "packages/EasyApiToken/src",
            "EonX\\EasyAsync\\": "packages/EasyAsync/src",
            "EonX\\EasyAwsCredentialsFinder\\": "packages/EasyAwsCredentialsFinder/src",
            "EonX\\EasyBankFiles\\": "packages/EasyBankFiles/src",
            "EonX\\EasyBatch\\": "packages/EasyBatch/src",
            "EonX\\EasyBatchNew\\": "packages/EasyBatch/src",
            "EonX\\EasyBugsnag\\": "packages/EasyBugsnag/src",
            "EonX\\EasyCore\\": "packages/EasyCore/src",
            "EonX\\EasyDecision\\": "packages/EasyDecision/src",
            "EonX\\EasyDecision\\Expressions\\": "packages/EasyDecision/packages/expressions/src",
            "EonX\\EasyDoctrine\\": "packages/EasyDoctrine/src",
            "EonX\\EasyEncryption\\": "packages/EasyEncryption/src",
            "EonX\\EasyErrorHandler\\": "packages/EasyErrorHandler/src",
            "EonX\\EasyEventDispatcher\\": "packages/EasyEventDispatcher/src",
            "EonX\\EasyHttpClient\\": "packages/EasyHttpClient/src",
            "EonX\\EasyLock\\": "packages/EasyLock/src",
            "EonX\\EasyLogging\\": "packages/EasyLogging/src",
            "EonX\\EasyNotification\\": "packages/EasyNotification/src",
            "EonX\\EasyPagination\\": "packages/EasyPagination/src",
            "EonX\\EasyPipeline\\": "packages/EasyPipeline/src",
            "EonX\\EasyPsr7Factory\\": "packages/EasyPsr7Factory/src",
            "EonX\\EasyRandom\\": "packages/EasyRandom/src",
            "EonX\\EasyRepository\\": "packages/EasyRepository/src",
            "EonX\\EasyRequestId\\": "packages/EasyRequestId/src",
            "EonX\\EasySecurity\\": "packages/EasySecurity/src",
            "EonX\\EasySchedule\\": "packages/EasySchedule/src",
            "EonX\\EasySsm\\": "packages/EasySsm/src",
            "EonX\\EasyTemplatingBlock\\": "packages/EasyTemplatingBlock/src",
            "EonX\\EasyTest\\": "packages/EasyTest/src",
            "EonX\\EasyUtils\\": "packages/EasyUtils/src",
            "EonX\\EasyWebhook\\": "packages/EasyWebhook/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyActivity\\Tests\\": "packages/EasyActivity/tests",
            "EonX\\EasyApiToken\\Tests\\": "packages/EasyApiToken/tests",
            "EonX\\EasyAsync\\Tests\\": "packages/EasyAsync/tests",
            "EonX\\EasyAwsCredentialsFinder\\Tests\\": "packages/EasyAwsCredentialsFinder/tests",
            "EonX\\EasyBankFiles\\Tests\\": "packages/EasyBankFiles/tests",
            "EonX\\EasyBatch\\Tests\\": "packages/EasyBatch/tests",
            "EonX\\EasyBatchNew\\Tests\\": "packages/EasyBatch/tests",
            "EonX\\EasyBugsnag\\Tests\\": "packages/EasyBugsnag/tests",
            "EonX\\EasyCore\\Tests\\": "packages/EasyCore/tests",
            "EonX\\EasyDecision\\Tests\\": "packages/EasyDecision/tests",
            "EonX\\EasyDecision\\Expressions\\Tests\\": "packages/EasyDecision/packages/expressions/tests",
            "EonX\\EasyDoctrine\\Tests\\": "packages/EasyDoctrine/tests",
            "EonX\\EasyEncryption\\Tests\\": "packages/EasyEncryption/tests",
            "EonX\\EasyErrorHandler\\Tests\\": "packages/EasyErrorHandler/tests",
            "EonX\\EasyEventDispatcher\\Tests\\": "packages/EasyEventDispatcher/tests",
            "EonX\\EasyHttpClient\\Tests\\": "packages/EasyHttpClient/tests",
            "EonX\\EasyLock\\Tests\\": "packages/EasyLock/tests",
            "EonX\\EasyLogging\\Tests\\": "packages/EasyLogging/tests",
            "EonX\\EasyNotification\\Tests\\": "packages/EasyNotification/tests",
            "EonX\\EasyPagination\\Tests\\": "packages/EasyPagination/tests",
            "EonX\\EasyPipeline\\Tests\\": "packages/EasyPipeline/tests",
            "EonX\\EasyPsr7Factory\\Tests\\": "packages/EasyPsr7Factory/tests",
            "EonX\\EasyRandom\\Tests\\": "packages/EasyRandom/tests",
            "EonX\\EasyRepository\\Tests\\": "packages/EasyRepository/tests",
            "EonX\\EasyRequestId\\Tests\\": "packages/EasyRequestId/tests",
            "EonX\\EasySecurity\\Tests\\": "packages/EasySecurity/tests",
            "EonX\\EasySchedule\\Tests\\": "packages/EasySchedule/tests",
            "EonX\\EasySsm\\Tests\\": "packages/EasySsm/tests",
            "EonX\\EasyTemplatingBlock\\Tests\\": "packages/EasyTemplatingBlock/tests",
            "EonX\\EasyTest\\Tests\\": "packages/EasyTest/tests",
            "EonX\\EasyUtils\\Tests\\": "packages/EasyUtils/tests",
            "EonX\\EasyWebhook\\Tests\\": "packages/EasyWebhook/tests"
        }
    },
    "replace": {
        "eonx-com/easy-api-token": "self.version",
        "eonx-com/easy-repository": "self.version",
        "eonx-com/easy-pagination": "self.version",
        "eonx-com/easy-psr7-factory": "self.version",
        "eonx-com/easy-random": "self.version",
        "eonx-com/easy-pipeline": "self.version",
        "eonx-com/easy-lock": "self.version",
        "eonx-com/easy-decision": "self.version",
        "eonx-com/easy-core": "self.version",
        "eonx-com/easy-doctrine": "self.version",
        "eonx-com/easy-encryption": "self.version",
        "eonx-com/easy-error-handler": "self.version",
        "eonx-com/easy-event-dispatcher": "self.version",
        "eonx-com/easy-http-client": "self.version",
        "eonx-com/easy-logging": "self.version",
        "eonx-com/easy-security": "self.version",
        "eonx-com/easy-test": "self.version",
        "eonx-com/easy-schedule": "self.version",
        "eonx-com/easy-ssm": "self.version",
        "eonx-com/easy-async": "self.version",
        "eonx-com/easy-webhook": "self.version",
        "eonx-com/easy-bugsnag": "self.version",
        "eonx-com/easy-request-id": "self.version",
        "eonx-com/easy-bank-files": "self.version",
        "eonx-com/easy-utils": "self.version"
    },
    "scripts": {
        "check-all": [
            "@check-ecs",
            "@check-phpstan",
            "@check-tests"
        ],
        "check-ecs": ".quality/vendor/bin/ecs check --ansi",
        "check-monorepo": "vendor/bin/monorepo-builder validate --ansi",
        "check-composer": "@composer validate --strict",
        "check-phpstan": ".quality/vendor/bin/phpstan analyse --error-format symplify --ansi --memory-limit=1000M",
        "check-tests": "vendor/bin/phpunit packages --coverage-text",
        "check-rector": ".quality/vendor/bin/rector process --ansi --config rector-ci.php --dry-run",
        "changelog": [
            "vendor/bin/changelog-linker dump-merges --since-id 491 --ansi",
            "vendor/bin/changelog-linker link --ansi",
            "vendor/bin/changelog-linker cleanup --ansi"
        ],
        "fix-ecs": ".quality/vendor/bin/ecs check --fix --ansi",
        "fix-rector": ".quality/vendor/bin/rector process --ansi --config rector-ci.php",
        "merge": "vendor/bin/monorepo-builder merge --ansi",
        "release": [
            "bin/monorepo clean-up-packages-vendor-dirs",
            "vendor/bin/monorepo-builder release"
        ],
        "propagate": "vendor/bin/monorepo-builder propagate --ansi",
        "split": "vendor/bin/monorepo-builder split --ansi",
        "post-install-cmd": [
            "cd .quality && composer install --dev --no-interaction"
        ]
    },
    "suggest": {
        "auth0/auth0-php": "To use the Auth0 implementation to handle your users identity",
        "doctrine/orm": "To use the Doctrine ORM implementation",
        "eonx-com/easy-pagination": "To use pagination within your repositories",
        "firebase/php-jwt": "To use the Firebase JWT driver to decode/encode tokens",
        "illuminate/database": "To use the Illuminate Eloquent implementation",
        "illuminate/pipeline": "To use the Illuminate Pipeline implementation"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "process-timeout": 0
    },
    "extra": {
        "eonx_docs": {
            "ignore": true
        },
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    }
}
