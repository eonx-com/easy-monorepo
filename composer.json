    "replace": {
        "eonx-com/easy-activity": "self.version",
        "eonx-com/easy-api-platform": "self.version",
        "eonx-com/easy-api-token": "self.version",
        "eonx-com/easy-async": "self.version",
        "eonx-com/easy-aws-credentials-finder": "self.version",
        "eonx-com/easy-bank-files": "self.version",
        "eonx-com/easy-batch": "self.version",
        "eonx-com/easy-bugsnag": "self.version",
        "eonx-com/easy-core": "self.version",
        "eonx-com/easy-decision": "self.version",
        "eonx-com/easy-doctrine": "self.version",
        "eonx-com/easy-encryption": "self.version",
